pragma solidity ^0.8.16;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "./iwavax.sol";

contract erc1155wavax is ERC1155Upgradeable, ERC1155BurnableUpgradeable, UUPSUpgradeable, OwnableUpgradeable{
    

    IWAVAX constant wavax = IWAVAX(0xB31f66AA3C1e785363F0875A1B74E27b85FD66c7); //testnet: 0x1D308089a2D1Ced3f1Ce36B1FcaF815b07217be3

    uint[50] __gap;
    function initialize() initializer public {
        __ERC1155_init("");
        __ERC1155Burnable_init();
        __UUPSUpgradeable_init();
        __Ownable_init();
    }

    function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}

    function name() public view virtual returns (string memory) {
        return "NFT Wrapped AVAX";
    }

    function symbol() public view virtual returns (string memory) {
        return "NFTWAVAX";
    }

    fallback() external payable {
        mintWithAvax(msg.value / 1 ether);
    }

    receive() external payable {
        mintWithAvax(msg.value / 1 ether);
    }

    function mint(uint256 amount) public {
        require(amount % 1 == 0 && amount > 0, "Amount must be a multiple of 1");
        wavax.transferFrom(msg.sender, address(this), amount * 1 ether);
        _mint(msg.sender, 1, amount, "");
    }

    function mintWithAvax(uint256 amount) public payable {
        require(amount % 1 == 0 && amount > 0, "Amount must be a multiple of 1");
        require(msg.value == amount * 1 ether, "Amount must be equal to msg.value");
        wavax.deposit{value: msg.value}();
        _mint(msg.sender, 1, amount, "");
    }

    function redeem(uint256 amount) public {
        require(amount % 1 == 0 && amount > 0, "Amount must be a multiple of 1");
        require(balanceOf(msg.sender, amount)> 0, "Not enough tokens");
        _burn(msg.sender, amount, 1);
        wavax.transfer(owner(), amount * 1 ether * 5/100);
        wavax.transfer(msg.sender, amount * 1 ether * 95/100);
    }

    function uri(uint256 tokenId) public view virtual override returns (string memory) {
        return string(abi.encodePacked("data:application/json;base64,", Base64Upgradeable.encode(bytes(abi.encodePacked('{"name":"', StringsUpgradeable.toString(tokenId), " WAVAX", '", "description":"', StringsUpgradeable.toString(tokenId), " WAVAX. Go to https://wavax.snowforge.xyz to wrap/unwrap.", '", "image":"', "https://raw.githubusercontent.com/ava-labs/bridge-tokens/main/avalanche-tokens/0xB31f66AA3C1e785363F0875A1B74E27b85FD66c7/logo.png", '"}')))));
    }

}