// NOTE: Aave Hedger only works with Aave V3, so no Ethereum for now

// All parent contracts must be OZ upgradeable-compatible
contract AaveHedger is UpgradeableHedgerBase {
  using EnumerableSet for EnumerableSet.AddressSet;

  /// @dev Aave pool address, acquired from the Aave address provider (getPool)
  IPool public aavePool;

  AaveOracle public oracle;

  /// @dev (Wrapped) Native Gas Token (e.g. WETH, WMATIC, WAVAX, etc.)
  IERC20 public nativeToken;

  /// @dev Enabled tokens (approvals set)
  EnumerableSet.AddressSet private enabledTokens;

  /// @dev Proxy initializer (constructor)
  function initialize() public initializer {
    // Call parent upgradeable contract initializers FIRST
    __Ownable_init_unchained();
    __Pausable_init_unchained();
    __UUPSUpgradeable_init_unchained();
  }

  function initContract(
    address _config,
    address _oracle,
    address _aavePoolAddressProvider,
    address _usdc,
    address _nativeToken
  ) public onlyOwner {
    config = Config(_config);
    paraswap = IParaSwapAugustus(0xDEF171Fe48CF0115B1d80b88dc8eAB59176FEe57);
    paraswapTokenProxy = paraswap.getTokenTransferProxy();
    setAaveConfig(_oracle, _aavePoolAddressProvider, _usdc, _nativeToken);
    // initiateAllowances();

    address[] memory tokens = new address[](1);
    tokens[0] = address(USDC);
    enableERCTokens(tokens);
  }

  /// @dev Initialize the contract (set up allowances, etc.)
  // function initiateAllowances() internal {
  //   // Manaully approve USDC in particular here (approval from the contract address)
  //   USDC.approve(address(paraswapTokenProxy), type(uint).max);
  //   // Allowance for Aave to transfer money from contract
  //   USDC.approve(address(aavePool), type(uint256).max);
  // }

  function setAaveConfig(
    address _oracle,
    address _aavePoolAddressProvider,
    address _usdc,
    address _nativeToken
  ) public onlyOwner {
    oracle = AaveOracle(_oracle);
    aavePool = IPool(IPoolAddressesProvider(_aavePoolAddressProvider).getPool());
    USDC = IERC20(_usdc);
    nativeToken = IERC20(_nativeToken);
  }

  /// @dev Deposit USDC directly as the collateral (mint vUSDC)
  function depositCollateral(uint256 amount) public override {
    require(USDC.allowance(msg.sender, address(this)) >= amount, 'Insufficient USDC allowance');
    require(USDC.balanceOf(msg.sender) >= amount, 'Insufficient USDC to transfer');
    require(USDC.transferFrom(msg.sender, address(this), amount), 'USDC transfer failed');
    aavePool.supply(address(USDC), amount, address(this), 0);
  }

  /// @dev `availableCollateral` is total collateral - debt, not accounting for LTV
  function canHedge(
    address token,
    address, // duplicate, but required for interface compatibility with Venus
    uint256 amount, // amount to short (borrow & swap)
    // add buffer to `amountUSDC` (ie. add a bit more collateral than needed, in case the executing block has slippage)
    // 0.01% => 1 // 0.1% => 10 // 1% => 100
    uint256 buffer
  ) public view returns (bool possible, uint256 availableCollateral, uint256 shortfall) {
    // NOTE: Data returned in decimals of USDC (1e8 in most cases)
    //       Reference: https://docs.aave.com/developers/core-contracts/pool#getuseraccountdata
    //       Returns: totalCollateralBase, totalDebtBase, availableBorrowsBase, currentLiquidationThreshold, ltv, healthFactor
    (uint totalCollateralBase, uint totalDebtBase, , , , ) = aavePool.getUserAccountData(address(this));
    // NOTE: Aave uses 10^8 decimals, so we divide by 1e12 to get 10^6 decimals (USDC).
    //       We calculate total raw collateral available, whereas `availableBorrowsBase` is LTV-included total collateral.
    availableCollateral = (totalCollateralBase - totalDebtBase) / 1e2;

    // 1e10 since totalDebtBase is 1e8 and newDebtBase is1e18
    uint newDebtBase = totalDebtBase * 1e10 + assetValue(token, amount); // existing debt + new debt
    // console.log('totalCollateralBase', totalCollateralBase * 1e10);
    // console.log('newDebtBase', newDebtBase);

    // 1e10 since totalCollateralBase is 1e8 and _canHedge param is 1e18
    (possible, shortfall) = _canHedge(
      token,
      newDebtBase, // total soon-to-be debt
      totalCollateralBase * 1e10, // total raw collateral
      buffer
    );
  }

  /// @dev Hedge and gives negative delta
  ///      Slippage is multiplied by 10^4, e.g. 0.01% => 100 // 0.05% => 500
  ///      From: https://github.com/aave/aave-v3-periphery/blob/master/contracts/adapters/paraswap/BaseParaSwapSellAdapter.sol#L42
  ///      ref: https://github.com/aave/aave-v3-periphery/blob/master/contracts/adapters/paraswap/BaseParaSwapBuyAdapter.sol#L40
  function hedge(
    IERC20 token, // example: ETH
    bytes memory swapCalldata,
    uint256 amountToSwapToken, // expected amount of token to hedge & swap
    uint256 minAmountToReceiveUSDC // min amount of USDC to receive
  ) public override onlyOwner returns (uint256 amountReceived) {
    aavePool.borrow(address(token), amountToSwapToken, 2, 0, address(this));
    amountReceived = _hedgeSwapStep(token, swapCalldata, amountToSwapToken, minAmountToReceiveUSDC);
  }

  /// @dev Unhedge by swapping USDC to borrowed token and repaying
  function payback(
    IERC20 token,
    bytes memory swapCalldata,
    uint256 maxAmountToSwapUSDC, // slippage dependent
    uint256 amountToReceiveToken // always constant
  ) public override onlyOwner returns (uint256 amountSold) {
    amountSold = _paybackSwapStep(token, swapCalldata, maxAmountToSwapUSDC, amountToReceiveToken);
    aavePool.repay(address(token), amountToReceiveToken, 2, address(this)); // 2: variable APY
  }

  /// @dev Close all hedges of all tokens and exit immediately
  function exit() public onlyOwner {}

  /// @dev Asset value in USDC (returned in 18 decimals)
  function assetValue(address token, uint256 amount) public view returns (uint256) {
    if (token == address(USDC)) return amount;
    // NOTE: Normalize the output to 18 decimals. Aave oracle returns in 8 decimals.
    //       Since `amount` for IERC20 is in 18 decimals, just divide by oracle decimals.
    return (amount * oracle.getAssetPrice(token)) / 1e8;
  }

  function getEnabledTokens() public view returns (address[] memory) {
    return enabledTokens.values();
  }

  function isEnabledToken(address token) public view returns (bool) {
    return enabledTokens.contains(token);
  }

  function enableERCTokens(address[] memory tokens) public onlyOwner {
    for (uint16 i = 0; i < tokens.length;) {
      enabledTokens.add(tokens[i]);
      IERC20(tokens[i]).approve(address(paraswapTokenProxy), type(uint).max);
      IERC20(tokens[i]).approve(address(aavePool), type(uint).max);
      unchecked {
        i++;
      }
    }
  }

  function disableERCTokens(address[] memory tokens) public onlyOwner {
    for (uint16 i = 0; i < tokens.length; ) {
      enabledTokens.remove(tokens[i]);
      IERC20(tokens[i]).approve(address(paraswapTokenProxy), 0);
      IERC20(tokens[i]).approve(address(aavePool), 0);
      unchecked {
        i++;
      }
    }
  }
}

