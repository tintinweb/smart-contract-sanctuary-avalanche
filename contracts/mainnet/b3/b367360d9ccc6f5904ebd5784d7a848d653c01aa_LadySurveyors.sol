pragma solidity ^0.8.13;

import "./BoilerplateERC721.sol";

contract LadySurveyors is BoilerplateERC721{

  mapping(address => uint) public whitelistedAddresses;
  mapping(address => uint) public permaWhitelist;
  mapping(address => uint) public freeMintList;
  mapping(address => uint) public freeMintCount;
  address public immutable premintAddress;
  uint public immutable maxPremint;
  uint public immutable maxFreeMint = 100;

  constructor(
    string memory _name,
    string memory _symbol,
    string memory _initBaseURI,
    address _royaltyAddress,
    uint96 _royaltiesPercentage,
    uint _maxSupply,
    uint _maxMintAmount,
    address _treasury,
    address _lpPair,
    address _premintAddress,
    uint _maxPremint
  )BoilerplateERC721(
    _name,
    _symbol,
    _initBaseURI,
    _royaltyAddress,
    _royaltiesPercentage,
    _maxSupply,
    _maxMintAmount,
    _treasury,
    _lpPair
  ) {
    premintAddress = _premintAddress;
    maxPremint = _maxPremint;
  }

  /*
  * @dev Mint one or many NFTs with a specific token and potential discount
  * @param _token      - Token address to be used for payment
  * @param _amount     - Amount of tokens to be minted
  * @param _collection - NFT collection address used for potential discount.
  *                      If no discount, set to zero address
  */
  function mint(address _token, uint _amount, address _collection) external payable override {
    _validateMint(_token,_amount);
    uint price = _getPrice(_token, _amount, _collection);
    //Minter is whitelisted, so we give discount
    if(whitelistedAddresses[msg.sender] != 0 || permaWhitelist[msg.sender] != 0) {
      price = price / 2;
    }
    //Minter is on free mint list, so make price 0
    if(freeMintList[msg.sender] != 0) {
      require(freeMintCount[msg.sender] + _amount <= 100, "Minting above maximum allowed amount for free mint");
      price = 0;
      freeMintCount[msg.sender] += _amount;
      //If the count for free mints equals 100 after the mint, then we remove from freelist so user can still mint for regular cost
      if(freeMintCount[msg.sender] == 100) {
        freeMintList[msg.sender] = 0;
      }
    }
    
    require(IERC20(_token).balanceOf(msg.sender) >= price, "Don't have enough tokens");
    require(IERC20(_token).transferFrom(msg.sender, address(this), price), "Did/Cannot Pay");

    //Minter is not on perma list and is whitelisted, so remove the discount
    if(permaWhitelist[msg.sender] == 0 && whitelistedAddresses[msg.sender] != 0) {
      whitelistedAddresses[msg.sender] = 0;
    }
    _mintInternal(_amount);
  }

  function setWhitelistedAddresses(address[] calldata _userAddresses) external onlyOwner {
    uint len = _userAddresses.length;
    for(uint i = 0; i < len; i++) {
      whitelistedAddresses[_userAddresses[i]] = 1;
    }
  }

  function setFreeMintAddresses(address[] calldata _userAddresses) external onlyOwner {
    uint len = _userAddresses.length;
    for(uint i = 0; i < len; i++) {
      freeMintList[_userAddresses[i]] = 1;
    }
  }

  function setPermaWhitelistAddresses(address[] calldata _userAddresses) external onlyOwner {
    uint len = _userAddresses.length;
    for(uint i = 0; i < len; i++) {
      permaWhitelist[_userAddresses[i]] = 1;
    }
  }

  function premintTokens() external onlyOwner {
    uint supply = totalSupply();
    uint len = 0;
    require(supply < maxPremint, "Too many tokens preminted");
    for(uint i = 1; i <= maxPremint; i++) {
      len = ids.length - index++;      
      ids[supply] = uint16(ids[len - 1] == 0 ? len - 1 : ids[len - 1]);
      ids[len - 1] = 0;
      _safeMint(premintAddress, supply + 1);
      supply++;
    }
  }

}

