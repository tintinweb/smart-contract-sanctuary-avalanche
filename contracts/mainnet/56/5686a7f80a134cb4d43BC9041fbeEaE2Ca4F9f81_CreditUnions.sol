pragma solidity ^0.8.17;








contract CreditUnions is AccessControl, Pausable {
    bytes32 public constant LIQUIDITY_PROVIDER = keccak256("LIQUIDITY_PROVIDER");

    struct Member {
        bool valid;
        uint256 totalDelegated;
        uint256 totalBorrowed;
        bool stable;
    }

    mapping(address=>Member) public members;

    IPool constant lendingPool = IPool(0x794a61358D6845594F94dc1DB02A252b5b4814aD);

    address usdcToken = 0xB97EF9Ef8734C71904D8002F8b6Bc66Dd9c48a6E;
    ICreditDelegationToken sUSDC = ICreditDelegationToken(0x307ffe186F84a3bc2613D1eA417A5737D69A7007);
    ICreditDelegationToken vUSDC = ICreditDelegationToken(0xFCCf3cAbbe80101232d343252614b6A3eE81C989);

    uint256 public availableUSDC;

    uint256 public totalBorrowed;

    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(LIQUIDITY_PROVIDER, msg.sender);
    }

    function createLoan(address _member, uint256 _total, bool stable) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(members[_member].valid, "Not a member");

        if (stable) {
            sUSDC.approveDelegation(_member, _total);
        } else {
            vUSDC.approveDelegation(_member, _total);
        }

        members[_member].stable = stable;
        members[_member].totalDelegated += _total;
        totalBorrowed += _total;
    }

    function borrowAfterLoanCreated() public {
        Member memory member = members[msg.sender];
        require(member.valid, "Not a member");
        require(member.totalDelegated > 0, "Loan not available");
        require(member.totalDelegated > member.totalBorrowed, "Loan already completed");

        uint256 difference = member.totalDelegated - member.totalBorrowed;

        members[msg.sender].totalBorrowed += difference;
        members[msg.sender].totalDelegated -= difference;

        lendingPool.borrow(usdcToken, difference, getInterestMode(member.stable), 0, address(this));
        IERC20(usdcToken).transfer(msg.sender, difference);
    }

    function repayLoan(address _member) external {
        require(members[_member].totalBorrowed > 0, "Invalid loan");

        uint256 total = members[_member].totalBorrowed;
        members[_member].totalBorrowed = 0;

        IERC20(usdcToken).transferFrom(msg.sender, address(this), total);

        totalBorrowed -= total;

        IERC20(usdcToken).approve(address(lendingPool), total);
        lendingPool.repay(usdcToken, total, getInterestMode(members[_member].stable), address(this));
    }

    function setMember(address _member, bool _enabled) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_enabled || members[_member].totalBorrowed == 0, "The member has an active loan");

        members[_member].valid = _enabled;
    }

    function setMembers(address[] memory _members, bool _enabled) external onlyRole(DEFAULT_ADMIN_ROLE) {
        for(uint i = 0; i < _members.length; i++) {
            require(_enabled || members[_members[i]].totalBorrowed == 0, "The member has an active loan");
            members[_members[i]].valid = _enabled;
        }
    }

    function addLiquidity(uint256 _total) external onlyRole(LIQUIDITY_PROVIDER) {
        IERC20(usdcToken).transferFrom(msg.sender, address(this), _total);

        supplyAAVE(_total);

        availableUSDC += _total;
    } 

    function removeLiquidity(uint256 _total) external onlyRole(LIQUIDITY_PROVIDER) {
        require(availableUSDC - totalBorrowed >= _total, "Unavailable amount");

        withdrawFromAAVE(_total, msg.sender);

        IERC20(usdcToken).transfer(msg.sender, _total);

        availableUSDC -= _total;
    }

    function supplyAAVE(uint256 _amount) internal {
        IERC20(usdcToken).approve(address(lendingPool), _amount);
        lendingPool.supply(usdcToken, _amount, address(this), 0);
    }

    function withdrawFromAAVE(uint256 _amount, address to) internal {
        lendingPool.withdraw(usdcToken, _amount, to);
    }

    function getInterestMode(bool stable) internal pure returns(uint256) {
        if (stable) {
            return 1;
        }
        return 2;
    }

    function _recoverTokens(uint256 _amount, address _asset) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(IERC20(_asset).transfer(msg.sender, _amount), 'Transfer failed');
    }

    function _recoverAVAX(uint256 _amount) public onlyRole(DEFAULT_ADMIN_ROLE) {
        payable(msg.sender).transfer(_amount);
    }

    function _recoverByWithdrawing(uint256 _amount) public onlyRole(DEFAULT_ADMIN_ROLE) {
        withdrawFromAAVE(_amount, msg.sender);
    }
}