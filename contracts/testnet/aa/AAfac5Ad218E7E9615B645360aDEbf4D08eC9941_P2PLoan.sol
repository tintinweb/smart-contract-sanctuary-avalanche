pragma solidity ^0.8.17;
import {IP2PLoan} from "../interfaces/IP2PLoan.sol";
import {IERC721Upgradeable} from "../openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import {IERC721ReceiverUpgradeable} from "../openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import {CountersUpgradeable} from "../openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import {Initializable} from "../openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {ContextUpgradeable} from "../openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import {GenericLogic} from "../libraries/logic/GenericLogic.sol";
import {IConfigProvider} from "../interfaces/IConfigProvider.sol";
import {DataTypes} from "../libraries/types/DataTypes.sol";
import {Errors} from "../libraries/helpers/Errors.sol";
import {IBNFT} from "../interfaces/IBNFT.sol";
import {IBNFTRegistry} from "../interfaces/IBNFTRegistry.sol";
import "hardhat/console.sol";

contract P2PLoan is
    Initializable,
    IP2PLoan,
    ContextUpgradeable,
    IERC721ReceiverUpgradeable
{
    using CountersUpgradeable for CountersUpgradeable.Counter;

    IConfigProvider private _provider;

    CountersUpgradeable.Counter private _loanIdTracker;
    mapping(uint256 => DataTypes.LoanData) private _loans;

    // nftAsset + nftTokenId => loanId
    mapping(address => mapping(uint256 => uint256)) private _nftToLoanIds;

    function initialize(IConfigProvider provider) external initializer {
        __Context_init();

        _provider = provider;

        // Avoid having loanId = 0
        _loanIdTracker.increment();
    }

    function initNft(address nftAsset) external override onlyLendFactory {
        IBNFTRegistry bnftRegistry = IBNFTRegistry(_provider.bnftRegistry());
        address bNftAddress = bnftRegistry.getBNFTAddresses(nftAsset);
        IERC721Upgradeable(nftAsset).setApprovalForAll(bNftAddress, true);
    }

    modifier onlyLendFactory() {
        require(
            _msgSender() == address(_getLendFactory()),
            Errors.LP_CALLER_NOT_LEND_POOL_CONFIGURATOR
        );
        _;
    }

    function _getLendFactory() internal view returns (address) {
        return IConfigProvider(_provider).lendManager();
    }

    function getLoan(uint256 loanId)
        external
        view
        override
        returns (DataTypes.LoanData memory loanData)
    {
        return _loans[loanId];
    }

    /**
     * @dev Create store a loan object with some params
     */
    function createLoan(DataTypes.BorrowParams memory params)
        external
        override
        onlyLendFactory
        returns (uint256)
    {
        require(
            _nftToLoanIds[params.nftAsset][params.nftTokenId] == 0,
            Errors.LP_NFT_HAS_USED_AS_COLLATERAL
        );

        uint256 loanId = _loanIdTracker.current();
        _loanIdTracker.increment();

        _nftToLoanIds[params.nftAsset][params.nftTokenId] = loanId;

        // transfer underlying NFT asset to pool and mint bNFT to onBehalfOf
        IERC721Upgradeable(params.nftAsset).safeTransferFrom(
            _msgSender(), // shopFactory
            address(this),
            params.nftTokenId
        );

        address bNftAddress = GenericLogic.getBNftAddress(
            _provider,
            params.nftAsset
        );
        if (
            !IERC721Upgradeable(params.nftAsset).isApprovedForAll(
                address(this),
                bNftAddress
            )
        ) {
            IERC721Upgradeable(params.nftAsset).setApprovalForAll(
                bNftAddress,
                true
            );
        }
        IBNFT(bNftAddress).mint(params.borrower, params.nftTokenId);

        // Save Info
        DataTypes.LoanData storage loan = _loans[loanId];
        loan.state = DataTypes.LoanState.Active;
        loan.loanId = loanId;
        loan.loanPrincipalAmount = params.loanPrincipalAmount;
        loan.nftTokenId = params.nftTokenId;
        loan.loanStartTime = block.timestamp; //_loanStartTime
        loan.loanDuration = params.loanDuration;
        loan.loanInterestRate = params.loanInterestRate;
        loan.nftAsset = params.nftAsset;
        loan.reserveAsset = params.reserveAsset;
        loan.borrower = params.borrower;
        loan.lender = params.lender;
        loan.isNative = params.isNative;

        emit LoanStarted(
            loanId,
            loan.borrower,
            loan.lender,
            loan.nftAsset,
            loan.nftTokenId,
            loan.reserveAsset,
            loan.loanPrincipalAmount,
            loan.loanDuration,
            loan.loanInterestRate
        );

        return (loanId);
    }

    /**
     * @dev Repay the given loan
     *
     * Requirements:
     *  - The caller must be a holder of the loan
     *  - The caller must send in principal + interest
     *  - The loan must be in state Active
     *
     * @param initiator The address of the user initiating the repay
     * @param loanId The loan getting burned
     */
    function repayLoan(
        address initiator,
        uint256 loanId,
        uint256 amount
    ) external override onlyLendFactory {
        // Must use storage to change state
        DataTypes.LoanData storage loan = _loans[loanId];

        // Ensure valid loan state
        require(
            loan.state == DataTypes.LoanState.Active,
            Errors.LPL_INVALID_LOAN_STATE
        );

        _nftToLoanIds[loan.nftAsset][loan.nftTokenId] = 0;

        address bNftAddress = GenericLogic.getBNftAddress(
            _provider,
            loan.nftAsset
        );
        IBNFT(bNftAddress).burn(loan.nftTokenId);

        IERC721Upgradeable(loan.nftAsset).safeTransferFrom(
            address(this),
            _msgSender(),
            loan.nftTokenId
        );
        delete _loans[loanId];
        emit LoanRepaid(
            initiator,
            loanId,
            loan.nftAsset,
            loan.nftTokenId,
            loan.reserveAsset,
            amount
        );
    }

    /**
     * @dev Liquidate the given loan
     *
     * Requirements:
     *  - The caller must send in principal + interest
     *  - The loan must be in state Active
     *
     * @param initiator The address of the user initiating the auction
     * @param loanId The loan getting burned
     */
    function liquidateLoan(
        address initiator,
        uint256 loanId,
        uint256 borrowAmount
    ) external override onlyLendFactory {
        DataTypes.LoanData storage loan = _loans[loanId];
        // Ensure valid loan state
        require(
            loan.state == DataTypes.LoanState.Active,
            Errors.LPL_INVALID_LOAN_STATE
        );

        _loans[loanId].state = DataTypes.LoanState.Defaulted;

        _nftToLoanIds[loan.nftAsset][loan.nftTokenId] = 0;

        // burn bNFT and transfer underlying NFT asset to user
        address bNftAddress = GenericLogic.getBNftAddress(
            _provider,
            loan.nftAsset
        );

        IBNFT(bNftAddress).burn(loan.nftTokenId);

        IERC721Upgradeable(loan.nftAsset).safeTransferFrom(
            address(this),
            _msgSender(),
            loan.nftTokenId
        );
        delete _loans[loanId];
        emit LoanLiquidated(
            initiator,
            loanId,
            loan.nftAsset,
            loan.nftTokenId,
            loan.reserveAsset,
            borrowAmount
        );
    }

    function getCollateralLoanId(address nftAsset, uint256 nftTokenId)
        external
        view
        override
        returns (uint256)
    {
        return _nftToLoanIds[nftAsset][nftTokenId];
    }

    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) external pure override returns (bytes4) {
        operator;
        from;
        tokenId;
        data;
        return IERC721ReceiverUpgradeable.onERC721Received.selector;
    }

    function totalDebtInReserve(uint256 loanId)
        external
        view
        override
        returns (
            address reserveAsset,
            uint256 totalDebt,
            uint256 principalAmount,
            uint256 interest,
            uint256 platformFee
        )
    {
        reserveAsset = _loans[loanId].reserveAsset;
        (totalDebt, interest, platformFee) = GenericLogic.calculateInterestInfo(
            GenericLogic.CalculateInterestInfoVars({
                principalAmount: _loans[loanId].loanPrincipalAmount,
                interestRate: _loans[loanId].loanInterestRate,
                platformFeeRate: _provider.platformFeeRate(),
                loanStartTime: _loans[loanId].loanStartTime,
                loanDuration: _loans[loanId].loanDuration,
                interestBlock: _provider.interestBlock()
            })
        );
        return (
            reserveAsset,
            totalDebt,
            _loans[loanId].loanPrincipalAmount,
            interest,
            platformFee
        );
    }
}

