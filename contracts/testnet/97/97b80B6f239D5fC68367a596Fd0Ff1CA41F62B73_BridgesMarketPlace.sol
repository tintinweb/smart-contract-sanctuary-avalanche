/// @custom:security-contact [emailprotected]
contract SNFT is
    Initializable,
    ERC1155Upgradeable,
    OwnableUpgradeable,
  ERC1155VotesUpgradeable,
    ERC1155BurnableUpgradeable,
    ERC2981Upgradeable,
    UUPSUpgradeable
{
