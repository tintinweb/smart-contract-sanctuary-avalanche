    address indexed snft,
    uint256 tokenId,
    address payToken,
    uint256 newPrice
  );
  event SNFTListingCanceled(
    address indexed seller,
    address indexed snft,
    uint256 tokenId
  );

  event OfferCreated(
    address indexed creator,
    address indexed snft,
    uint256 tokenId,
    uint256 quantity,
    address payToken,
    uint256 pricePerItem,
    uint256 deadline
  );
  event OfferCanceled(
    address indexed creator,
    address indexed snft,
    uint256 tokenId
  );

  event OfferAccepted(
    address indexed creator,
    address indexed snft,
    uint256 tokenId
  );

  event ProceedsUpdated(
    address indexed account,
    uint256 previousVal,
    uint256 currentVal
  );
  event UpdatePlatformFee(uint16 platformFee);
  event UpdatePlatformRoyaltyFee(uint16 platformRoyaltyFee);
  event UpdatePlatformFeeRecipient(address payable platformFeeRecipient);
  // Modifers
  modifier ifNotListed(
    address _snft,
    uint256 _tokenId,
    address _owner
  ) {
    Listing memory listing = snftListing[_snft][_tokenId][_owner];
    if (listing.pricePerItem > 0) {
      revert BridgesMarketPlace__AlreadyListed(_snft, _tokenId);
    }
    _;
  }
  modifier isListed(
    address _snft,
    uint256 _tokenId,
    address _owner
  ) {
    Listing memory listing = snftListing[_snft][_tokenId][_owner];
    if (listing.pricePerItem <= 0) {
      revert BridgesMarketPlace__NotListed(_snft, _tokenId);
    }
    _;
  }

  modifier isOwner(address _snft, uint256 _tokenId) {
    console.log('isOwner', IBridgesSNFT(_snft).balanceOf(msg.sender, _tokenId));
    if (IBridgesSNFT(_snft).balanceOf(msg.sender, _tokenId) < 1) {
      revert BridgesMarketPlace__NotSNFTOwner();
    }
    _;
  }

  modifier isApproved(address _snft, address _owner) {
    if (!IBridgesSNFT(_snft).isApprovedForAll(_owner, address(this))) {
      revert BridgesMarketPlace__NotApprovedForMarketplace();
    }
    _;
  }

  modifier validListing(address _snft, uint256 _tokenId) {
    Listing memory listedItem = snftListing[_snft][_tokenId][msg.sender];
    _validOwner(_snft, _tokenId, msg.sender, listedItem.quantity);

    if (_getCurrentTS() < listedItem.startingTime) {
      revert BridgesMarketPlace__ItemNotBuyableYet();
    }
    _;
  }

  modifier offerExists(
    address _snft,
    uint256 _tokenId,
    address _creator
  ) {
    Offer memory offer = offers[_snft][_tokenId][_creator];

    if (offer.quantity < 1 && offer.deadline < _getCurrentTS()) {
      revert BridgesMarketPlace__OfferNotExistsOrExpired();
    }

    _;
  }

  modifier offerAlreadyExists(
    address _snft,
    uint256 _tokenId,
    address _creator
  ) {
    Offer memory offer = offers[_snft][_tokenId][_creator];

    if (offer.quantity > 1 || offer.deadline >= _getCurrentTS()) {
      revert BridgesMarketPlace__OfferAlreadyCreated();
    }
    _;
  }

  modifier isSNFT(address _snft) {
    if (!IBridgesSNFT(_snft).supportsInterface(INTERFACE_ID_ERC1155)) {
      revert BridgesMarketPlace__NotSnftAsset();
    }
    _;
  }

  modifier isNotSeller(
    address _snft,
    address _spender,
    uint256 _tokenId
  ) {
    Listing memory listing = snftListing[_snft][_tokenId][msg.sender];
    if (listing.exist && msg.sender == _spender) {
      revert BridgesMarketPlace__AlreadyOwnedAsset(_snft, _tokenId);
    }
    _;
  }

  receive() external payable {}

