    address private registry;
    uint96 public feeNumerator;
    uint256[] private batchedIds;

    /// @notice  Errors
    error SNFT__NotUsingBridgesMarketplace();

    /// @notice Address registry
    IBridgesRegistry public addressRegistry;

    /// @notice modifiers
    modifier ifNotMarket(address operator) {
        console.log("operator",operator);
        if (addressRegistry.marketplace() != operator) {
            console.log("BridgesSNFT__NotUsingBridgesMarketplace()");
            revert SNFT__NotUsingBridgesMarketplace();
        }
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        string memory myuri,
        uint96 _feeNumerator,
        address _registry,
        address _owner
    ) public initializer {
        __ERC1155_init(myuri);
        __Ownable_init();
        __ERC1155Burnable_init();
        __UUPSUpgradeable_init();

        feeNumerator = _feeNumerator;
        _setDefaultRoyalty(_owner, feeNumerator);
        addressRegistry = IBridgesRegistry(_registry);
    }

    receive() external payable {}

    function setURI(string memory newuri) public onlyOwner {
        _setURI(newuri);
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public onlyOwner {
        _mintBatch(to, ids, amounts, data);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public override {
        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()),
            "ERC1155: caller is not token owner nor approved"
        );
        _safeTransferFrom(from, to, id, amount, data);
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal virtual override(ERC1155Upgradeable) ifNotMarket(operator) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
        console.log("operator",operator);
        // console.log(data);
    }

    function _afterTokenTransfer(
    address operator,
    address from,
    address to,
    uint256[] memory ids,
    uint256[] memory amounts,
    bytes memory data
  ) internal override(ERC1155Upgradeable, ERC1155VotesUpgradeable) {
    for (uint256 i = 0; i < ids.length; i++) {
      _transferVotingUnits(from, to, amounts[i]);
    }

    super._afterTokenTransfer(operator, from, to, ids, amounts, data);
  }

    function _authorizeUpgrade(
        address newImplementation
    ) internal override onlyOwner {}

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(ERC1155Upgradeable, ERC2981Upgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

