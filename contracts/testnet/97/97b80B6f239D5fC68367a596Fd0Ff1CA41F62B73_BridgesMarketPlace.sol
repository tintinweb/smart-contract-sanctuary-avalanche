    __UUPSUpgradeable_init();
    __ReentrancyGuard_init();

    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _grantRole(PAUSER_ROLE, msg.sender);
    _grantRole(UPGRADER_ROLE, msg.sender);

    platformFee = _platformFee;
    platformRoyaltyFee = _platformRoyaltyFee;
    feeRecipient = _feeRecipient;
    addressRegistry = IBridgesRegistry(_addressRegistry);
  }

  function pause() public onlyRole(PAUSER_ROLE) {
    _pause();
  }

  function unpause() public onlyRole(PAUSER_ROLE) {
    _unpause();
  }

  function _authorizeUpgrade(
    address _newImplementation
  ) internal override(UUPSUpgradeable) onlyRole(UPGRADER_ROLE) {}

  ////////////////////
  // Main Function //
  ///////////////////

  /*
   * @notice Method for listing your phisycal property (SNFT) on the Bridges Marketplace
   * @param snft: Address of the SNFT
   * @param tokenId: The Token ID of the SNFT
   * @param price: Sale Price for the listed SNFT
   * @dev Allow owners to maintain ownership of their assets while listing them
   */

  function listSNFT(
    address _snft,
    uint256 _tokenId,
    uint256 _qty,
    address _payToken,
    uint256 _pricePerItem,
    uint256 _startingTime
  )
    external
    isSNFT(_snft)
    ifNotListed(_snft, _tokenId, msg.sender)
    isOwner(_snft, _tokenId)
    isApproved(_snft, msg.sender)
  {
    console.log('comeon');
    if (_pricePerItem <= 0) {
      revert BridgesMarketPlace__PriceMustBeAboveZero();
    }

    _listSNFT(_snft, _tokenId, _qty, _payToken, _pricePerItem, _startingTime);
  }

  function _listSNFT(
    address _snft,
    uint256 _tokenId,
    uint256 _qty,
    address _payToken,
    uint256 _pricePerItem,
    uint256 _startingTime
  ) internal {
    if (IBridgesSNFT(_snft).balanceOf(msg.sender, _tokenId) < _qty) {
      revert BridgesMarketPlace__NotEnoughSnfts();
    }

    _validPayToken(_payToken);

    snftListing[_snft][_tokenId][msg.sender] = Listing(
      _qty,
      _payToken,
      _pricePerItem,
      _startingTime,
      true
    );

    if (minters[_snft][_tokenId] == msg.sender) {
      initialSalePrice[_snft][_tokenId] = _pricePerItem;
    }

    if (IBridgesSNFT(_snft).hasRole(DEFAULT_ADMIN_ROLE, msg.sender)) {
      minters[_snft][_tokenId] = msg.sender;
    }

    emit SNFTListed(
      _snft,
      _tokenId,
      _pricePerItem,
      _qty,
      _payToken,
      _startingTime
    );
  }

  /// @notice Method for canceling listed SNFT
  function cancelListing(
    address _snft,
    uint256 _tokenId
  )
    external
    whenNotPaused
    isOwner(_snft, _tokenId)
    isListed(_snft, _tokenId, msg.sender)
    validListing(_snft, _tokenId)
  {
    _cancelListing(_snft, _tokenId);
  }

  function _cancelListing(address _snft, uint256 _tokenId) private {
    delete (snftListing[_snft][_tokenId][msg.sender]);
    emit SNFTListingCanceled(msg.sender, _snft, _tokenId);
  }

  /// @notice Method for updating listed NFT
  /// @param _snft Address of NFT contract
  /// @param _tokenId Token ID of NFT
  /// @param _payToken payment token
  /// @param _newPrice New sale price for each item
  function updateListing(
    address _snft,
    uint256 _tokenId,
    address _payToken,
    uint256 _newPrice
  ) external nonReentrant isListed(_snft, _tokenId, msg.sender) {
    console.log('updateListing');
    Listing storage listedItem = snftListing[_snft][_tokenId][msg.sender];
    console.log(listedItem.quantity);
    _validOwner(_snft, _tokenId, msg.sender, listedItem.quantity);

    _validPayToken(_payToken);

    listedItem.payToken = _payToken;
    listedItem.pricePerItem = _newPrice;
    emit SNFTListingUpdated(msg.sender, _snft, _tokenId, _payToken, _newPrice);
  }

  /*
   * @notice Method for buying your phisycal property (SNFT) on the Bridges Marketplace
   * @param _snft: Address of the SNFT
   * @param _tokenId: The Token ID of the SNFT
   * @param _qty: Quantity of units of SNFT to buy
   * @param _payToken: currency
   * @param _seller: seller of the listing
   */

  function buySNFT(
    address _snft,
    uint256 _tokenId,
    uint256 _qty,
    address _payToken,
    address _seller
  )
    public
    isApproved(_snft, _seller)
    isListed(_snft, _tokenId, _seller)
    isNotSeller(_snft, msg.sender, _tokenId)
    nonReentrant
    whenNotPaused
  {
    console.log('buy some');
    currentSNFT = _snft;
    _buySNFT(_tokenId, _qty, _payToken, _seller);
  }

  function _buySNFT(
    uint256 _tokenId,
    uint256 _qty,
    address _payToken,
    address _seller
  ) private {
    console.log('buy some2');

    uint256 id = _tokenId;
    uint256 qty = _qty;
    address payToken = _payToken;
    address seller = _seller;
    // Be sure there is enough money
    Listing memory listedSNFT = snftListing[currentSNFT][id][seller];

    _testIfSufficientFunds(payToken, qty, listedSNFT, id);

    // Gross Profit
    uint256 profitPerItem;

    if (listedSNFT.pricePerItem < initialSalePrice[currentSNFT][id]) {
      profitPerItem = initialSalePrice[currentSNFT][id].sub(
        listedSNFT.pricePerItem
      );
    } else {
      profitPerItem = 0;
    }

    uint256 price = listedSNFT.pricePerItem.mul(qty);
    uint256 totalProfit = profitPerItem.mul(qty);

    uint256 feeAmount = price.mul(platformFee).div(1e3);
    console.log(feeAmount);

    uint256 grossToSeller;
    if (totalProfit > feeAmount) {
      grossToSeller = totalProfit.sub(feeAmount);
    } else {
      grossToSeller = feeAmount;
    }
    console.log(grossToSeller);
    address minter = minters[currentSNFT][id];
    console.log(minter);

    (, uint256 royaltyFee) = IBridgesSNFT(currentSNFT).royaltyInfo(
      id,
      grossToSeller
    );

    if (minter == address(0)) {
      initialSalePrice[currentSNFT][id] = listedSNFT.pricePerItem;
    }

    // Secondary Market
    (
      uint256 calculatedPlatformRoyaltyFeeAmount,
      uint256 calculatedRoyaltyFee
    ) = secondaryMarket(minter, royaltyFee, payToken);

    feeAmount = feeAmount.add(calculatedPlatformRoyaltyFeeAmount);
    grossToSeller = grossToSeller - calculatedRoyaltyFee;

    sendAndUpdateProceeds(
      payToken,
      feeRecipient,
      seller,
      feeAmount,
      grossToSeller
    );
    // remove entry listing
    delete (snftListing[currentSNFT][id][seller]);

    // TransferItems memory tItems = TransferItems(seller, id, qty, '0x0');

    _transfer(seller, id, qty);
    console.log('SNFTSold1');
    console.log(listedSNFT.pricePerItem.mul(qty));

    _emitTx(id, qty, seller, listedSNFT.pricePerItem);
  }

  function sendAndUpdateProceeds(
    address payToken,
    address _feeRecipient,
    address seller,
    uint256 feeAmount,
    uint256 grossToSeller
  ) internal {
    IERC20Upgradeable(payToken).safeTransferFrom(
      msg.sender,
      _feeRecipient,
      feeAmount
    );

    Proceeds memory sellerProceeds = proceeds[seller];
    proceeds[seller].amount = proceeds[seller].amount + grossToSeller;
    proceeds[seller].payToken = payToken;
    Proceeds memory newSellerProceeds = proceeds[seller];
    emit ProceedsUpdated(
      seller,
      sellerProceeds.amount,
      newSellerProceeds.amount
    );
  }

  function secondaryMarket(
    address minter,
    uint256 royaltyFee,
    address payToken
  )
    internal
    returns (
      uint256 calculatedPlatformRoyaltyFeeAmount,
      uint256 calculatedRoyaltyFee
    )
  {
    if (minter != address(0) && royaltyFee != 0) {
      uint256 platformRoyaltyFeeAmount = royaltyFee.mul(platformRoyaltyFee).div(
        1e3
      );

      // minter's royalty proceeds
      IERC20Upgradeable(payToken).safeTransferFrom(
        msg.sender,
        address(this),
        royaltyFee
      );

      Proceeds memory minterProceeds = proceeds[minter];
      proceeds[minter].amount = royaltyFee;
      proceeds[minter].payToken = payToken;
      emit ProceedsUpdated(
        minter,
        minterProceeds.amount,
        proceeds[minter].amount
      );

      return (platformRoyaltyFeeAmount, royaltyFee);
    }
  }

  function _testIfSufficientFunds(
    address _payToken,
    uint256 qty,
    Listing memory listed,
    uint256 _tokenId
  ) internal view {
    if (
      IERC20Upgradeable(_payToken).balanceOf(msg.sender) <
      listed.pricePerItem.mul(qty)
    ) {
      revert BridgesMarketPlace__BuyerCreditCardDeclined(
        currentSNFT,
        _tokenId,
        _payToken
      );
    }
  }

  function _emitTx(
    uint256 id,
    uint256 qty,
    address seller,
    uint256 price
  ) internal {
    TransactionParties memory parties = TransactionParties(seller, msg.sender);
    TransactionItem memory item = TransactionItem(qty, currentSNFT, id);

    SoldItem memory sold = SoldItem(parties, item, price);

    emit SNFTSold(sold);
  }

  function _transfer(address from, uint256 id, uint256 amount) internal {
    // solhint-disable-next-line avoid-low-level-calls
    SNFT(payable(currentSNFT)).safeTransferFrom(
      from,
      msg.sender,
      id,
      amount,
      '0x0'
    );
  }

  function withdrawProceeds() external whenNotPaused {
    Proceeds memory myProceeds = proceeds[msg.sender];

    if (myProceeds.amount <= 0) {
      revert BridgesMarketPlace__NoProceeds();
    }
    proceeds[msg.sender].amount = 0;

    bool success = IERC20Upgradeable(proceeds[msg.sender].payToken).transfer(
      msg.sender,
      proceeds[msg.sender].amount
    );
    // solhint-disable-next-line avoid-low-level-calls
    // (bool success, ) = payable(msg.sender).call{value: myProceeds.amount}('');
    if (!success) {
      revert BridgesMarketPlace__FailedTransfer();
    }
  }

  function createOffer(
    address _snft,
    uint256 _tokenId,
    address _payToken,
    uint256 _quantity,
    uint256 _pricePerItem,
    uint256 _deadline
  ) external offerAlreadyExists(_snft, _tokenId, msg.sender) {
    if (!IBridgesSNFT(_snft).supportsInterface(INTERFACE_ID_ERC1155)) {
      revert BridgesMarketPlace__InvalidSnftAddress();
    }

    (, , , uint256 startTime, , bool resulted, ) = IBridgesAuction(
      addressRegistry.auction()
    ).getAuction(_snft, _tokenId);

    if (!(startTime == 0 || resulted == true)) {
      revert BridgesMarketPlace__CannotPlaceOfferAuctionActive();
    }
    console.log('_getCurrentTS()');
    console.log(_getCurrentTS());
    console.log(_deadline);

    if (_deadline <= _getCurrentTS()) {
      revert BridgesMarketPlace__InvalidExpiration();
    }

    _validPayToken(_payToken);

    offers[_snft][_tokenId][msg.sender] = Offer(
      _payToken,
      _quantity,
      _pricePerItem,
      _deadline
    );

    emit OfferCreated(
      msg.sender,
      _snft,
      _tokenId,
      _quantity,
      _payToken,
      _pricePerItem,
      _deadline
    );
  }

  /// @notice Method for canceling the offer
  /// @param _snft NFT contract address
  /// @param _tokenId TokenId
  function cancelOffer(
    address _snft,
    uint256 _tokenId
  ) external offerExists(_snft, _tokenId, msg.sender) {
    delete (offers[_snft][_tokenId][msg.sender]);
    emit OfferCanceled(msg.sender, _snft, _tokenId);
  }

  /// @notice Method for accepting the offer
  /// @param _snft NFT contract address
  /// @param _tokenId TokenId
  /// @param _creator Offer creator address
  function acceptOffer(
    address _snft,
    uint256 _tokenId,
    address _creator
  ) external offerExists(_snft, _tokenId, _creator) {
    Offer memory offer = offers[_snft][_tokenId][_creator];

    _validOwner(_snft, _tokenId, msg.sender, offer.quantity);

    emit OfferAccepted(_creator, _snft, _tokenId);
  }

  ////////////////////////////
  /// Internal and Private ///
  ////////////////////////////

  function _getCurrentTS() internal virtual returns (uint256) {
    return block.timestamp;
  }

  function _validPayToken(address _payToken) internal {
    if (
      _payToken == address(0) ||
      (addressRegistry.payTokenRegistry() == address(0) &&
        !IPayTokenRegistry(addressRegistry.payTokenRegistry()).enabled(
          _payToken
        ))
    ) {
      revert BridgesMarketPlace__InvalidPayToken();
    }
  }

  function _validOwner(
    address _snft,
    uint256 _tokenId,
    address _owner,
    uint256 _qty
  ) internal {
    if (IBridgesSNFT(_snft).supportsInterface(INTERFACE_ID_ERC1155)) {
      IBridgesSNFT snft = IBridgesSNFT(_snft);
      console.log(snft.balanceOf(_owner, _tokenId));
      if (snft.balanceOf(_owner, _tokenId) < _qty) {
        revert BridgesMarketPlace__NotItemOwner();
      }
    } else {
      revert BridgesMarketPlace__InvalidSnftAddress();
    }
  }

