  /// @notice SnftAddress -> Token ID -> Initial Sale Price
  mapping(address => mapping(uint256 => uint256)) public initialSalePrice;

  /// @notice SnftAddress -> Token ID -> Owner -> Listing item
  mapping(address => mapping(uint256 => mapping(address => Listing)))
    public snftListing;

  /// @notice SnftAddress -> Token ID -> Offerer -> Offer
  mapping(address => mapping(uint256 => mapping(address => Offer)))
    public offers;

  /// @notice seller -> proceeds
  mapping(address => Proceeds) public proceeds;

  /// @notice Platform fee
  uint16 public platformFee;

  /// @notice Platform fee
  uint16 public platformRoyaltyFee;

  /// @notice Platform fee receipient
  address payable public feeRecipient;

  /// @notice Address registry
  IBridgesRegistry public addressRegistry;

  address private currentSNFT;

  // Events
  event SNFTListed(
    address indexed snft,
    uint256 indexed tokenId,
    uint256 pricePerItem,
    uint256 qty,
    address payToken,
    uint256 startingTime
  );

  event SNFTSold(SoldItem sold);

  event SNFTListingUpdated(
