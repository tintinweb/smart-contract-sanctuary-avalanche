contract NGBankVaultFacet is WithStorage {
    
    event LPRemoved();
    event LPAdded();
    event ClaimRewards();
    event LPClaimRewards();


    modifier onlyAdminOrCore() {
        require(
            (msg.sender == gs().diamondAddress) ||
                (ads().allowedAccounts[msg.sender]) ||
                (msg.sender == LibDiamond.contractOwner()),
            "Only the Core or Admin addresses can fiddle with artifacts."
        );
        _;
    }

    modifier onlyAdmin() {
        require(
            (ads().allowedAccounts[msg.sender]) ||
                (msg.sender == LibDiamond.diamondStorage().contractOwner),
            "cannot access admin functions if not admin");
        _;
    }

    modifier onlyAPI() {
        require(
            (ads().allowedAPIAccounts[msg.sender]),
            "cannot access API functions if not API");
        _;
    }

    modifier onlyAdminOrAPI() {
        require(
            (ads().allowedAccounts[msg.sender]) ||
                (msg.sender == LibDiamond.diamondStorage().contractOwner) ||
                    (ads().allowedAPIAccounts[msg.sender]),
            "cannot access API functions if not API or Admin");
        _;
    }

    modifier notPaused() {
        require(!gs().paused, "game is currently paused");
        _;
    }

    modifier resourceAddresses() {
        require(gs().goldAvaxResourceAddress != address(0), "Must have goldavax LP address");
        require(gameConstants().BANK_VAULT_REWARD_PER_SECONDS > 0, "BANK_VAULT_REWARD_PER_SECONDS is not set");
        _;
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool() public notPaused resourceAddresses {
        if (block.timestamp <= gs().avaxGoldLPLastRewardTimestamp) {
            return;
        }
        if (gs().totalAvaxGoldLPBalance == 0) {
            gs().avaxGoldLPLastRewardTimestamp = block.timestamp;
            return;
        }
        
        uint256 secondsSinceUpdate = block.timestamp - gs().avaxGoldLPLastRewardTimestamp;
        // console.log("updatePool - secondsSinceUpdate", secondsSinceUpdate);
        uint256 tokenRewards = secondsSinceUpdate * gameConstants().BANK_VAULT_REWARD_PER_SECONDS * 1e18 / 100;
        // console.log("updatePool - tokenRewards", tokenRewards);
        gs().accRewardTokenPerShare = gs().accRewardTokenPerShare + tokenRewards * 1e30 / gs().totalAvaxGoldLPBalance;
        // console.log("updatePool - accRewardTokenPerShare", gs().accRewardTokenPerShare);
        gs().avaxGoldLPLastRewardTimestamp = block.timestamp;
    }

   
     // Add LP
     function addLP(uint256 _amount) public notPaused resourceAddresses  {
        require(_amount > 0, "no _amount");
        require(gs().avaxGoldLPBalance[msg.sender] == 0, "Cant add LP if you already have LP");

        updatePool();

        gs().avaxGoldLPBalance[msg.sender] += _amount;
        gs().totalAvaxGoldLPBalance += _amount;

        if (gs().avaxGoldLPLastRewardTimestamp == 0) {
            gs().avaxGoldLPLastRewardTimestamp = block.timestamp;
        }
        
        uint256 balanceOf = gs().avaxGoldLP.balanceOf(msg.sender);
        require(balanceOf >= _amount, "Looks like the amount is incorrect");

        // uint256 allowance = gs().avaxGoldLP.allowance(msg.sender, address(this));
        // require(allowance >= _amount, "You have not set an allowance");

        gs().avaxGoldLP.transferFrom(address(msg.sender), address(this), _amount);
        
        gs().avaxGoldLPRewardDebt[msg.sender] = gs().avaxGoldLPBalance[msg.sender] * gs().accRewardTokenPerShare / 1e30;

        emit LPAdded();
     }

     // Remove LP
     function removeLP() public notPaused resourceAddresses  {
        require(gs().avaxGoldLPBalance[msg.sender] > 0, "You dont have any LP to remove");

        updatePool();

        claimLPRewards();
        uint256 _amount = gs().avaxGoldLPBalance[msg.sender];
        gs().avaxGoldLPBalance[msg.sender] -= _amount;
        gs().totalAvaxGoldLPBalance -= _amount;
        gs().avaxGoldLP.transfer(address(msg.sender), _amount);

        gs().avaxGoldLPRewardDebt[msg.sender] = 0;
        gs().avaxGoldLPRewardBalance[msg.sender] = 0;

        emit LPRemoved();
     }

     // Balance LP
     function balanceLP() public view notPaused resourceAddresses returns(uint256) {
        return gs().avaxGoldLPBalance[msg.sender];
     }
     function balanceLPAvaxGold() public view notPaused resourceAddresses returns(uint256) {
        return gs().avaxGoldLP.balanceOf(msg.sender);
     }

     // Current Ratio
     function currentLPRatio() public view notPaused resourceAddresses returns(uint256) {
        require(gs().avaxGoldLPBalance[msg.sender] > 0, "No LP balance");

        return gs().avaxGoldLPBalance[msg.sender] * 1000 / gs().totalAvaxGoldLPBalance;
     }

     // Current Rewards
     function currentRewards() public view notPaused resourceAddresses returns(uint256) {
        // require(gs().avaxGoldLPBalance[msg.sender] > 0, "No LP balance");

        // uint256 secondsSinceUpdate = block.timestamp - gs().avaxGoldLPLastRewardTimestamp;
        // return currentLPRatio() * secondsSinceUpdate * gameConstants().BANK_VAULT_REWARD_PER_SECONDS / 1000 / 100;

        uint256 accRewardTokenPerShare = gs().accRewardTokenPerShare;
        // console.log("currentRewards - gs().accRewardTokenPerShare", gs().accRewardTokenPerShare);
        if (block.timestamp > gs().avaxGoldLPLastRewardTimestamp && gs().totalAvaxGoldLPBalance != 0) {
            uint256 secondsSinceUpdate = block.timestamp - gs().avaxGoldLPLastRewardTimestamp;
            // console.log("currentRewards - secondsSinceUpdate", secondsSinceUpdate);
            
            uint256 tokenReward = secondsSinceUpdate * gameConstants().BANK_VAULT_REWARD_PER_SECONDS  * 1e18 / 100;
            // console.log("currentRewards - gs().totalAvaxGoldLPBalance", gs().totalAvaxGoldLPBalance);
            // console.log("currentRewards - tokenReward", tokenReward);
            accRewardTokenPerShare = accRewardTokenPerShare + tokenReward * 1e30 / gs().totalAvaxGoldLPBalance;
            // console.log("currentRewards - accRewardTokenPerShare", accRewardTokenPerShare);
        }
        return (gs().avaxGoldLPBalance[msg.sender] * accRewardTokenPerShare / 1e30) - gs().avaxGoldLPRewardDebt[msg.sender] - gs().avaxGoldLPRewardBalance[msg.sender];
     }

     // Claim rewards
     function claimLPRewards() public notPaused resourceAddresses  {
        require(gs().avaxGoldLPBalance[msg.sender] > 0, "You dont have any LP so cant claim");
        uint256 pendingReward = currentRewards();
        if (pendingReward > 0) {
            gs().avaxGoldLPRewardBalance[msg.sender] = pendingReward;
            // mint
            gs().goldResource.mint(msg.sender, pendingReward);
        }
        
        emit LPClaimRewards();
     }


    

}

