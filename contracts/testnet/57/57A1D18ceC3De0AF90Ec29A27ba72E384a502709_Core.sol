		uint i = 0;
		uint _length = pairAddresses.length;
		JoeV2_1PairInfo[] memory result = new JoeV2_1PairInfo[](_length);
		for(i=0; i<_length; i++) {
			JoeV2_1PairInfo memory pairInfo = getJoeV2_1PairInfo(pairAddresses[i]);
			result[i] = pairInfo;
		}
		return result;
