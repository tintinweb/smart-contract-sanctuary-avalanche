			_variableFeeControl,
			_volatilityAccumulator,
			_baseFactor,
			_binStep,
			_bins
		);
		return _pairInfo;
	}

	struct WoofiPairInfo {
		WoofiPairCoinInfo[] infos;
	}

	struct WoofiPairCoinInfo {
		uint256 reserve;
		uint128 price;
		uint64 coeff;
		uint64 spread;
		bool feasible;
	}

	IWooracleV2 woofiOracle = IWooracleV2(0x9ACA557590F5020BDA4Ba63065Fc3A1253Bf8000);
	address[] woofiPoolCoins = 
		[0xB97EF9Ef8734C71904D8002F8b6Bc66Dd9c48a6E,
		0x9702230A8Ea53601f5cD2dc00fDBc13d4dF4A8c7,
		0xB31f66AA3C1e785363F0875A1B74E27b85FD66c7,
		0x152b9d0FdC40C096757F570A51E494bd4b943E50,
		0x49D5c2BdFfac6CE2BFdB6640F4F80f226bc10bAB];


	function getWoofiPairInfos(address[] memory pairAddresses) view public returns (WoofiPairInfo[] memory) {
		uint i = 0;
		uint _length = pairAddresses.length;
		WoofiPairInfo[] memory result = new WoofiPairInfo[](_length);
		for(i=0; i<_length; i++) {
			WoofiPairInfo memory pairInfo = getWoofiPairInfo(pairAddresses[i]);
			result[i] = pairInfo;
		}
		return result;
	}

	function getWoofiPairInfo(address pairAddress) view public returns (WoofiPairInfo memory) {
		IWooPPV2 _pair = IWooPPV2(pairAddress);
		WoofiPairCoinInfo[] memory _infos = new WoofiPairCoinInfo[](woofiPoolCoins.length);

		for(uint i=0; i < woofiPoolCoins.length; i++) {
			uint256 _reserve = _pair.poolSize(woofiPoolCoins[i]);
			IWooracleV2.State memory _state = woofiOracle.state(woofiPoolCoins[i]);

			_infos[i] = WoofiPairCoinInfo(
				_reserve,
				_state.price,
				_state.coeff,
				_state.spread,
				_state.woFeasible
			);
		}

		WoofiPairInfo memory _pairInfo = WoofiPairInfo(
			_infos
		);

		return _pairInfo;
	}

	function swap(uint256 amountIn, uint256 minAmountOut, address[] memory pairs, PairType[] memory pairTypes, IERC20[] memory coins) onlyOwner payable public{
		uint256 amount;
		uint256 amountOut;
		for(uint i=0; i<pairs.length; i++) {
			address recipient = i + 1 == pairs.length ? msg.sender : address(this);
			coins[i].safeTransferFrom(msg.sender, pairs[i], amount);
			if(pairTypes[i] == PairType.JoeV2_1) {
				ILBPair pair = ILBPair(pairs[i]);
				bool swapForY = coins[i+1] == ILBPair(pair).getTokenY();

				(uint256 amountXOut, uint256 amountYOut) = ILBPair(pair).swap(swapForY, recipient).decode();
				if (swapForY) amountOut = amountYOut;
				else amountOut = amountXOut;
			} else if (pairTypes[i] == PairType.Woofi) {
				IWooPPV2 pair = IWooPPV2(pairs[i]);
				amountOut = pair.swap(address(coins[i]), address(coins[i+1]), amount, 0, recipient, recipient);
			}

			amountIn = amountOut;
		}

		if (minAmountOut > amountOut) revert Core__InsufficientAmountOut(minAmountOut, amountOut);
	}
}

