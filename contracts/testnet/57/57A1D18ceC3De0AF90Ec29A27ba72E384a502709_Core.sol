		uint256[] memory result = new uint256[](tokenAddresses.length+1);
		result[0] = accountAddress.balance;
		for (uint256 i = 0; i < tokenAddresses.length; i++) {
			IERC20 token = IERC20(tokenAddresses[i]);
			result[i+1] = token.balanceOf(accountAddress);
