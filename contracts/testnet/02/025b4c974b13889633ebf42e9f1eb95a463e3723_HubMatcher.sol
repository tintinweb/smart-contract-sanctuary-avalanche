// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.13;

import "layer-zero/interfaces/ILayerZeroEndpoint.sol";
import "layer-zero/interfaces/ILayerZeroReceiver.sol";
import "layer-zero/interfaces/ILayerZeroUserApplicationConfig.sol";

import "forge-std/console.sol";

import "./libs/FullMath.sol";

contract HubMatcher is 
    ILayerZeroReceiver,
    ILayerZeroUserApplicationConfig
{

  uint160 private constant PRC = uint160(uint(keccak256("PRC")));
  address private constant ZOD = address(uint160(uint(keccak256("ZOD"))));
  address private constant MKR = address(uint160(uint(keccak256("MKR"))));
  address private constant FIN = address(uint160(uint(keccak256("FIN"))));
  address private constant GAP = address(uint160(uint(keccak256("GAP"))));
  address private constant CUT = address(uint160(uint(keccak256("CUT"))));
  address private constant CLT = address(uint160(uint(keccak256("CLT"))));

  uint private constant MAX = type(uint).max;

  uint constant LZ_CHOKE = 10;
  uint constant LZ_SLICE = 12;

  uint16 lz_cid;
  ILayerZeroEndpoint public lz;

  event Log(uint256);
  event Log(address);

  // outgoing constants for spokes
  uint constant LZ_RESOLVE = uint(keccak256("RESOLVE"));
  uint constant LZ_SETTLE = uint(keccak256("SETTLE"));
  uint constant LZ_INIT = uint(keccak256("INIT"));

  // incoming constants for hub
  uint constant LZ_PAIR = uint(keccak256("PAIR"));
  uint constant LZ_PRIME = uint(keccak256("PRIME"));
  uint constant LZ_VERIFY = uint(keccak256("VERIFY"));
  uint constant LZ_XRS = uint(keccak256("XRS"));

  // incoming and outgoing constants from and to spokes
  uint constant LZ_CONNECT = uint(keccak256("CONNECT"));

  // outgoing for price pointer
  uint constant LZ_PRICE = uint(keccak256("PRICE"));

  struct Stream {
    TapeData                 data0;
    TapeData                 data1;
    mapping (uint => TapeIn) tape0;
    mapping (uint => TapeIn) tape1;
  }

  struct TapeData {
    uint    hd; // streaming, head for iteration starting point
    uint    tl; // streaming, tail for bounding iteration and appending 
    uint  colt; // streaming, total seized collateral to disperse at end
    bool  mkrs; // streaming, if tape is for maker orders or not
    uint point; // streaming, order id for signifying new head at spoke
  }

  struct LoopData {
    uint    hd;
    uint    tl;
    uint    ix;
    uint   ixx;
    uint   ixo;
    uint  pile;
    bool  inco;
    bool  mkrs;
    uint point;
  }

  mapping(uint => Stream) public streams;

  // token0 => token 1 => epoch => exchange rate
  // allows setting address(0) => address(0) => _epoch_-1 to PRC to signify 
  // pricing has arrived in that epoch
  mapping (address => mapping(address => mapping(uint => uint160))) public xroots;

  // token => chain => canonical token for pricing
  mapping (address => mapping(uint16 => address)) internal canon;

  struct TapeIn {
    address u;   // FIN, TIP, MKR signifiers or user address
    uint256 n0;  // if TIP, total capital. If ORD(er), capital
    uint256 n1;  // if TIP, collateral.    If ORD(er), id
  }

  struct TapeOut {
    address u;   // FIN, CUT, GAP, MKR, CLT signifiers or user address
    uint256 n0;  // units or id if u is FIN
  }

  struct Primer {
    uint    prime0;
    uint    prime1;
    address t0;
    uint16  c0;
    address t1;
    uint16  c1; 
  }

  struct Priming {
    uint tag;
    uint epo;
  }

  struct XR {
    address t0;
    address t1;
    uint160 n;
  }

  Priming[] public primings;
  XR[]      public xrprimings;

  function primingslen () public returns (uint) { return primings.length; }
  function xrprimingslen () public returns (uint) { return xrprimings.length; }

  mapping(uint => Primer) public primers;
  mapping(address => mapping(address => mapping(uint => bool))) xrprimed;

  // mapping an address to a chain id to an address
  // used for lookups to and from canonical tokens 
  mapping (address => mapping(uint => address)) public converter;

  mapping(uint => address) public spokes;

  uint16  internal PRICER_C;
  address internal PRICER;

  uint internal _epoch_;
  uint internal _epochmoment_;
  uint public epochspan;

  constructor (address _lz, uint16 _lz_cid) {

    _epoch_ = 1;
    epochspan = 60;
    _epochmoment_ = block.timestamp;

    lz = ILayerZeroEndpoint(_lz);
    lz_cid = _lz_cid;

  }

  function epoch () public view returns (uint) {

    return _epoch_ + (( block.timestamp - _epochmoment_ ) / epochspan);

  }

  function epochmoment () public view returns (uint) {

    uint news = (block.timestamp - _epochmoment_) / epochspan;

    return _epochmoment_ + ( epochspan * news );

  }

  function _ticktockepoch_ () internal {

    uint news = (block.timestamp - _epochmoment_) / epochspan;

    if (0 < news) {

      _epoch_ += news;

      _epochmoment_ += news * epochspan;

    }

  }

  function setpricer (uint16 _c, address _p) public {

    PRICER_C = _c;
    PRICER = _p;

  }

  function inductspoke (uint16 _cid, address _spoke) public payable {

    _ticktockepoch_();

    spokes[_cid] = _spoke;

    _send(_cid, _spoke, msg.sender,
      abi.encode(LZ_INIT, _epoch_, epochspan, _epochmoment_), 
      bytes("")
    );

  }


  function setcanonconverter (address ying, uint16 cid, address yang) public {

    converter[ying][cid] = yang;
    converter[yang][cid] = ying;

  }

  /// @notice this will start the settlement process on all of the primed spokes
  /// @dev    as primings come in during the pricing epoch, they are added to 
  ///         the primings array which this function iterates through
  ///         if a pair is eligible to be matched, messages are dispatched to 
  ///         the spokes to collect their orders and the priming is deleted 
  ///         from the list.
  ///         if a pair is not eligible to be matched, it is still in the pricing
  ///         epoch so is left in the list.
  function resolve() public {

    _ticktockepoch_();

    uint cut = _epoch_ - 1;
    uint i; uint maxix = primings.length;

    while (i < maxix) {

      Priming storage priming = primings[i];

      if (priming.epo < cut) {

        Primer storage primer = primers[priming.tag];

        _send(primer.c0, spokes[primer.c0], address(this),
          abi.encode(LZ_RESOLVE, primer.t0, primer.t1, primer.c1), bytes(""));

        _send(primer.c1, spokes[primer.c1], address(this),
          abi.encode(LZ_RESOLVE, primer.t1, primer.t0, primer.c0), bytes(""));

      }

      unchecked { i++; }

    }

    uint ixo = 0;
    uint len = i = xrprimings.length;
    maxix = i == 0 ? 0 : i - 1;

    XR[] memory xrs = new XR[](i);

    if (i > 0) for (i--;;) {
      if (xrprimings[i].n < uint160(cut)) {
        xrs[ixo++] = xrprimings[i];
        if (maxix != i) xrprimings[i] = xrprimings[maxix];
        if (maxix != 0) maxix--;
        xrprimings.pop();
      }
      if (i == 0) break; else i--;
    }

    assembly { mstore(xrs, sub(mload(xrs), sub(len, ixo))) }

    _send(PRICER_C, PRICER, msg.sender,
      abi.encode(LZ_PRICE, uint32(_epochmoment_), xrs), bytes(""));

  }


  function pair (bytes memory payload) internal {

    _ticktockepoch_();

    ( ,uint16          src_c,
       address         src_t,
       uint16          dst_c,
       address         dst_t,
       TapeIn[] memory stape
    ) = abi.decode(payload, (uint, uint16, address, uint16, address, TapeIn[]));

    uint gas = gasleft();

    _pair(src_c, src_t, dst_c, dst_t, stape);

    console.log("gas used in pairing", gas - gasleft());

  }

  /// @notice to be called with incoming orders from either side of spoke pair
  ///         will dispatch messages with settlement information for both sides
  /// @dev    this function intakes an order tape containing flags indicating 
  ///         the tape state (with constants FIN and MKR) and orders consisting
  ///         of the user, amount and link index from its source.
  ///         after determining which of tape0 and tape1 are the source and 
  ///         destination tapes, when there is no destination tape awaiting 
  ///         then the incoming tape is persisted to memory. 
  ///         if there is a destination tape awaiting then the incoming tape 
  ///         is matched and one of the two tapes is fully exhausted. if the 
  ///         destination tape is exhausted, then the remainder of the source 
  ///         tape is persisted to memory. but if the source tape is exhausted
  ///         then all that needsto be done is updating the destination tape
  ///         metadata.
  ///         the source tape is the tape incoming from the message
  ///         the destination tape is the tape waiting in persistence
  ///         the return value souts (source outs) are the matched orders for
  ///         the destination spoke to settle to.
  ///         the return value douts (destination outs) are the matched orders
  ///         for the source spoke to settle to.
  ///         the incoming MKR flag is used to indicate that matching is now
  ///           happening against makers from the respective spoke. it contains
  ///           an indication of how much seized collateral there has been so far
  ///           since there is a possibility that the MKR flag indicates the end
  ///           of matching when the opposing tape has been matching from makers 
  ///           by the time the tape in question meets its MKR flag 
  ///         the outgoing MKR flag is used to indicate that the spokes are now
  ///           settling from their maker orders as well as indicating the new head
  ///           for their trader's linked list. 
  ///         the outgoing flag CUT is used to indicate that an order was only
  ///           partially matched the last order. a CUT flag can be at the end of
  ///           the outgoing tape before either a GAP or a FIN flag.
  ///         the outgoing flag GAP is used to indicate that matching has hit a 
  ///           chokepoint such as gas or maximum length in the outgoing tapes 
  ///           (also decided by gas expenditure at the spoke settlement functions).
  ///           it comes with an indication of the current index for the linked list
  ///           being matched at the spoke so that a cut may be performed if necessary.
  ///           if the traders are being matched from, then this current index is the 
  ///           new head for the traders linked list.
  ///         the outgoing flag CLT is used to indicate collateral is being dispersed
  ///           which happens in scenarios where there are traders and makers that 
  ///           had a reasonable expectation to be matched but were unsatisfied due 
  ///           to failures of their opposing sides to fund margin orders. these
  ///           circumstances are indicated with the n0 member of a FIN or MKR flag
  ///           which records how much seized collateral there has been when the
  ///           orders were locked. the remianing orders on the opposing tape
  ///           are written to their respective outgoing tape to the extent that 
  ///           collateral is present to compensate them.
  ///         the outgoing flag FIN is used to indicate that the matching has been 
  ///           finished for both sides of the pair. it contains the last index 
  ///           that was matched which serves as the index from which to remit 
  ///           capital from, if the orders being matched were not from the makers
  ///           then this index is the new head of the traders linked list.
  function _pair(
    uint16  src_c, 
    address src_t, 
    uint16  dst_c, 
    address dst_t,
    TapeIn[] memory inc_stape
  ) internal {

    Stream storage stream = streams[tag(src_c, src_t, dst_c, dst_t)];
    LoopData memory sd; LoopData memory dd;

    TapeOut[] memory souts = new TapeOut[](LZ_SLICE); // src orders being sent to dst
    TapeOut[] memory douts = new TapeOut[](LZ_SLICE); // dst orders being sent to src

    if (xroots[ZOD][ZOD][_epoch_-1] == PRC) {

      uint160 xroot = src_t < dst_t 
        ? xroots[converter[src_t][src_c]][converter[dst_t][dst_c]][_epoch_-1]
        : xroots[converter[dst_t][dst_c]][converter[src_t][src_c]][_epoch_-1];

      if (xroot == 0) xroot = 0x1000000000000000000000000;

      {
        if (src_t < dst_t) {

          TapeData storage std = stream.data0; TapeData storage dtd = stream.data1;
          sd = LoopData(std.hd, std.tl, std.hd, std.hd, 0, 0, false, std.mkrs, std.point);
          dd = LoopData(dtd.hd, dtd.tl, dtd.hd, dtd.hd, 0, 0, false, dtd.mkrs, dtd.point);

        } else {

          TapeData storage std = stream.data1; TapeData storage dtd = stream.data0;
          sd = LoopData(std.hd, std.tl, std.hd, std.hd, 0, 0, false, std.mkrs, std.point);
          dd = LoopData(dtd.hd, dtd.tl, dtd.hd, dtd.hd, 0, 0, false, dtd.mkrs, dtd.point);

        }
      }

      mapping (uint => TapeIn) storage stape = src_t < dst_t ? stream.tape0 : stream.tape1;
      mapping (uint => TapeIn) storage dtape = src_t < dst_t ? stream.tape1 : stream.tape0;

      if (dd.tl != 0) {

        if (sd.ix == sd.tl) { 
          sd.inco = true; 
          sd.tl = inc_stape.length;
          sd.ix = sd.ixx = 0;
        }

        TapeIn memory s = sd.inco ? inc_stape[sd.ix] : stape[sd.ix];
        TapeIn memory d = dtape[dd.ix];

        bool zerosrc = src_t < dst_t;

        if (s.u != FIN && s.u != MKR) {
          sd.pile += s.n0;
          s.n0 = quote(xroot, zerosrc, s.n0);
        }
        if (d.u != FIN && d.u != MKR) {
          dd.pile += d.n0;
          d.n0 = quote(xroot, !zerosrc, d.n0);
        }

        while (true) {

          if (sd.ix != sd.ixx) { 
            s = sd.inco ? inc_stape[sd.ixx = sd.ix] : stape[sd.ixx = sd.ix]; 
            if (s.u != FIN && s.u != MKR) ( sd.pile += s.n0, s.n0 = quote(xroot, zerosrc, s.n0) ); 
          }
          if (dd.ix != dd.ixx) { 
            d = dtape[dd.ixx = dd.ix]; 
            if (d.u != FIN && d.u != MKR) ( dd.pile += d.n0, d.n0 = quote(xroot, !zerosrc, d.n0) );
          }

          if (s.u == FIN || s.u == GAP) 
              if (sd.inco || inc_stape.length == 0) ( sd.hd = sd.tl, sd.ix = MAX );
            else ( sd.tl = inc_stape.length, sd.hd = sd.ix, sd.ix = 0, sd.ixx = 1, sd.inco = true );
          if (d.u == FIN || d.u == GAP) ( dd.hd = dd.ix, dd.ix = MAX );

          if (s.u == MKR) if (sd.tl != ++sd.ix || !dd.mkrs || sd.ixo != LZ_CHOKE)
                ( sd.mkrs = true, douts[dd.ixo++] = TapeOut(MKR, sd.point));
              else if (sd.inco || inc_stape.length == 0) ( sd.hd = sd.tl, sd.ix = MAX );
            else ( sd.tl = inc_stape.length, sd.hd = sd.tl, sd.ix = 0, sd.ixx = 1, sd.inco = true );
          if (d.u == MKR) if (dd.tl != ++dd.ix || !sd.mkrs || dd.ixo != LZ_CHOKE) 
                        ( dd.mkrs = true, souts[sd.ixo++] = TapeOut(MKR, dd.point));
                      else ( dd.hd = dd.ix, dd.ix = MAX );

          if (s.n0 != 0 && s.n0 <= dd.pile) {
            sd.point = s.n1; dd.pile -= s.n0; souts[sd.ixo++] = TapeOut(s.u, s.n0);
            if (sd.tl < ++sd.ix || sd.ixo == LZ_CHOKE) {
                if (sd.inco || inc_stape.length == 0) ( sd.hd = sd.tl, sd.ix = MAX );
              else ( sd.tl = inc_stape.length, sd.hd = sd.tl, sd.ix = 0, sd.ixx = 1, sd.inco = true );
            }
          }

          if (d.n0 != 0 && d.n0 <= sd.pile) {

            dd.point = d.n1; sd.pile -= d.n0; douts[dd.ixo++] = TapeOut(d.u, d.n0);
            if (dd.tl < ++dd.ix || dd.ixo == LZ_CHOKE) ( dd.hd = dd.ix, dd.ix = MAX );

          }

          if (sd.ix == MAX && dd.ix == MAX) { 

            douts[dd.ixo++] = TapeOut(FIN, sd.point);
            souts[sd.ixo++] = TapeOut(FIN, dd.point); break;

          } else if (sd.ix == MAX) {

            dd.hd = dd.ix;

            if (0 < sd.pile) {
              d.n0           -= sd.pile; 
              d.n0            = quote(xroot, zerosrc, d.n0);
              dtape[dd.ix]    = d;
              douts[dd.ixo++] = TapeOut(d.u, sd.pile);
              souts[sd.ixo++] = TapeOut(CUT, sd.pile);
            }

            if (s.u == FIN || dd.mkrs && s.u == MKR) {

              uint collat = s.n0;

              if (0 < collat) {

                douts[dd.ixo++] = TapeOut(CLT, 0);

                while (dd.ixo != MAX) {

                  d = dtape[dd.ix];

                  if (d.u != FIN && d.u != MKR) {

                    if (collat >= d.n0) {
                      douts[dd.ixo++] = TapeOut(d.u, d.n0);
                      collat -= d.n0; 
                    } else {
                      douts[dd.ixo++] = TapeOut(d.u, collat);
                      collat = 0;
                    }

                  } else if (d.u == FIN) break;

                  if (dd.tl < ++dd.ix || dd.ixo == LZ_CHOKE) dd.ixo = MAX;

                }
              }

              douts[dd.ixo++] = TapeOut(FIN, sd.point);
              souts[sd.ixo++] = TapeOut(FIN, dd.point);

            } else ( douts[dd.ixo++] = TapeOut(GAP, sd.point),
                     souts[sd.ixo++] = TapeOut(GAP, dd.point) ); break;

          } else if (dd.ix == MAX) {

            sd.hd = sd.ix;

            if (0 < dd.pile) { 
              s.n0 -= dd.pile;
              s.n0  = quote(xroot, !zerosrc, s.n0);
              if (!sd.inco) stape[sd.ix] = s;
              else      inc_stape[sd.ix] = s;
              souts[sd.ixo++] = TapeOut(s.u, dd.pile);
              douts[dd.ixo++] = TapeOut(CUT, dd.pile);
            }

            if (d.u == FIN || sd.mkrs && d.u == MKR) {

              uint collat = d.n0;

              if (0 < collat) {

                souts[sd.ixo++] = TapeOut(CLT, 0);

                while (sd.ixo != MAX) {

                  s = sd.inco ? inc_stape[sd.ix] : stape[sd.ix];

                  if (s.u != FIN && s.u != MKR) {

                    if (collat >= s.n0) {
                      collat -= s.n0; 
                      souts[sd.ixo++] = TapeOut(s.u, s.n0);
                    } else souts[sd.ixo++] = TapeOut(s.u, collat);

                  } if (s.u == FIN) break;

                  if (sd.tl < ++sd.ix || sd.ixo == LZ_CHOKE) 
                    if (sd.inco || inc_stape.length == 0) sd.ixo = MAX;
                    else (sd.inco = true, sd.hd = sd.ix, sd.ix = 0, sd.tl = inc_stape.length );

                }
              }

              souts[sd.ixo++] = TapeOut(FIN, dd.point);
              douts[dd.ixo++] = TapeOut(FIN, sd.point);

            } else ( souts[sd.ixo++] = TapeOut(GAP, dd.point),
                     douts[dd.ixo++] = TapeOut(GAP, sd.point) ); break;

          }
        }

        if (src_t < dst_t) {
          TapeData storage std = stream.data0; TapeData storage dtd = stream.data1;
          std.hd = sd.hd; std.mkrs = sd.mkrs; std.point = sd.point;
          dtd.hd = dd.hd; dtd.mkrs = dd.mkrs; dtd.point = dd.point;
        } else {
          TapeData storage std = stream.data1; TapeData storage dtd = stream.data0;
          std.hd = sd.hd; std.mkrs = sd.mkrs; std.point = sd.point;
          dtd.hd = dd.hd; dtd.mkrs = dd.mkrs; dtd.point = dd.point;
        }

      }

    }

    if (douts[0].u != address(0)) {

      { uint dixout = dd.ixo; assembly { mstore(douts, sub(mload(douts), sub(LZ_SLICE, dixout))) } }

      bytes memory payload = abi.encode(LZ_SETTLE, src_t, dst_t, dst_c, douts);

      _send(src_c, spokes[src_c], address(this), payload, bytes(""));

    }

    if (souts[0].u != address(0)) {

      { uint sixout = sd.ixo; assembly { mstore(souts, sub(mload(souts), sub(LZ_SLICE, sixout))) } }

      bytes memory payload = abi.encode(LZ_SETTLE, dst_t, src_t, src_c, souts);

      _send(dst_c, spokes[dst_c], address(this), payload, bytes(""));

    }

    if (!sd.inco) ( sd.ix = 0, sd.tl = inc_stape.length );

    // if there are leftover stapes that is because all of the dtapes have been
    // paired on the outgoing tapes, which means the stapes must be persisted
    if (sd.ix != sd.tl && sd.ix != MAX) {

      mapping (uint => TapeIn) storage src_tape 
        = src_t < dst_t ? stream.tape0 : stream.tape1;

      uint tl = src_t < dst_t ? stream.data0.tl : stream.data1.tl;
      uint ix = sd.ix; uint len = sd.tl;

      while (ix < len) src_tape[tl++] = inc_stape[ix++];

      if (src_t < dst_t) stream.data0.tl = tl;
      else               stream.data1.tl = tl;

    } 

  }

  function quote(
    uint160 xroot, 
    bool zero, 
    uint base
  ) internal pure returns (uint256 quote_) {

      // don't overflow for precision
      if (xroot <= type(uint128).max) {
        uint256 sqr = uint256(xroot) * xroot;
        quote_ = zero
          ? FullMath.mulDiv(sqr, base, 1 << 192)
          : FullMath.mulDiv(1 << 192, base, sqr);
      } else {
        uint256 sqr = FullMath.mulDiv(xroot, xroot, 1 << 64);
        quote_ = zero
          ? FullMath.mulDiv(xroot, base, 1 << 128)
          : FullMath.mulDiv(1 << 128, base, xroot);
      }

  }

  function xrs (bytes memory payload) internal {

    // there could be one XR for many pairs, since these pairs are going from chain to chain
    ( , XR[] memory _xrs ) = abi.decode(payload, (uint, XR[]));

    uint len = _xrs.length;
    for (uint i = 0; i < len; i++) 
      xroots[_xrs[i].t0][_xrs[i].t1][_epoch_-1 ] = _xrs[i].n;

    xroots[ZOD][ZOD][_epoch_-1] = PRC;

    uint cut = _epoch_ - 1;
    uint i = primings.length;
    uint maxix = i == 0 ? 0 : i - 1;

    if (i > 0) for (i--;;) {

      Priming storage priming = primings[i];

      // if it is not last element then the last element was not eligible
      // for execution. swap this with last element and pop it for removal
      if (priming.epo < cut) {

        Primer storage primer = primers[primings[i].tag];

        TapeIn[] memory tape = new TapeIn[](0);

        _pair(primer.c0, primer.t0, primer.c1, primer.t1, tape);

        if (maxix != i) primings[i] = primings[maxix]; 
        if (maxix != 0) maxix--;
        primings.pop(); 

      } 

      if (i == 0) break; 
      else { unchecked { i--; }}

    }

  }

  function prime (bytes memory payload) internal {

    _ticktockepoch_();

    // epo is the epoch for the orders being primed which is at all times
    // at least one epoch ago, meaning those orders are currently being priced
    // or an edge case has occurred where they are eligible for settlement
    ( ,uint16 src_c, address src_t, uint16  dst_c, address dst_t, uint epo) 
      = abi.decode(payload, (uint, uint16, address, uint16, address, uint));

    ( Primer storage primer, uint tag ) = getprimer(src_c, src_t, dst_c, dst_t);

    uint inc; uint opp; 
    if (src_c == primer.c0) 
         (inc = primer.prime0 = epo, opp = primer.prime1);
    else (inc = primer.prime1 = epo, opp = primer.prime0);

    if (inc == opp) {

      primings.push(Priming(tag, inc));

      address t0 = converter[src_t][src_c];
      address t1 = converter[dst_t][dst_c];
      if (t0 > t1) (t0,t1) = (t1,t0);

      if (!xrprimed[t0][t1][epo]) {
        xrprimings.push(XR(t0,t1,uint160(epo)));
        xrprimed[t0][t1][epo] = true;
      }

    }

  }

  function connect (bytes memory _payload) internal {

    ( ,uint16 src, uint16 dst ) = abi.decode(_payload, ( uint, uint16, uint16 ));

    address src_spoke = spokes[src];
    address dst_spoke = spokes[dst];

    require(src_spoke != address(0), "src not inducted");
    require(dst_spoke != address(0), "dst not inducted");

    if (dst_spoke != address(0)) {

      _send(src, src_spoke, address(this),
        abi.encode(LZ_CONNECT, dst, dst_spoke),
        bytes("")
      );

      _send(dst, dst_spoke, address(this),
        abi.encode(LZ_CONNECT, src, src_spoke),
        bytes("")
      );

    }

  }

  function getprimer ( uint16  c0, address t0, uint16  c1, address t1) 
    internal returns (Primer storage primer, uint tag_) {

    tag_ = tag(c0,t0,c1,t1);

    primer = primers[tag_];

    if (primer.t0 == address(0)) {
      (primer.c0, primer.c1) = cidsort(c0, c1);
      (primer.t0, primer.t1) = toksort(t0, t1); }

  }

  function cidsort(
    uint16 c0,
    uint16 c1
  ) public pure returns (uint16 c0_, uint16 c1_) {

    if (c0 > c1) (c0, c1) = (c1, c0); c0_ = c0; c1_ = c1;

  }

  function toksort(
    address t0,
    address t1
  ) public pure returns (address t0_, address t1_) {

    if (t0 > t1) (t0, t1) = (t1, t0); t0_ = t0; t1_ = t1;

  }

  function tag(
    uint16  c0,
    address t0,
    uint16  c1,
    address t1
  ) public pure returns (uint key) {

    ( c0, c1 ) = cidsort(c0,c1);
    ( t0, t1 ) = toksort(t0,t1);

    return uint256(
      keccak256(abi.encodePacked(c0, t0, c1, t1)));

  }

  function forceResumeReceive(uint16 _srcChainId, bytes calldata _srcAddress) public {
      lz.forceResumeReceive(_srcChainId, _srcAddress);
  }

  function setReceiveVersion(uint16 _version) public {
      lz.setReceiveVersion(_version);
  }

  function setSendVersion(uint16 _version) public {
      lz.setSendVersion(_version);
  }

  function setConfig(uint16 _version, uint16 _chainId, 
     uint256 _configType, bytes calldata _config) public {

      lz.setConfig(_version, _chainId, _configType, _config);

  }

  function lzReceive(
    uint16 src_c,
    bytes calldata _src_addr,
    uint64 _nonce,
    bytes calldata _payload
  ) public {

    uint _type_ = abi.decode(_payload, (uint));

    if        (_type_ == LZ_PAIR)     return pair(_payload);
    else if   (_type_ == LZ_PRIME)    return prime(_payload);
    else if   (_type_ == LZ_CONNECT)  return connect(_payload);
    else if   (_type_ == LZ_XRS)      return xrs(_payload);

  }

  function _send (
    uint16 _cid, 
    address _to, 
    address _refund,
    bytes memory _payload, 
    bytes memory _params
  ) virtual internal {

    lz.send{value: address(this).balance}(
      _cid,
      abi.encodePacked(_to, address(this)),
      _payload,
      payable(_refund),
      address(0),
      _params
    );

  }

  function estimateFees(
    uint16 _dstChainId,
    address _userApplication,
    bytes calldata _payload,
    bool _payInZRO,
    bytes calldata _adapterParams
  ) external view returns (uint256 nativeFee, uint256 zeroFee) {

    (nativeFee, zeroFee) = lz.estimateFees(
      _dstChainId,
      _userApplication,
      _payload,
      false,
      bytes("")
    );

  }

  function tapes (
    uint16  c0, 
    address t0, 
    uint16  c1, 
    address t1,
    uint256 ix
  ) public view returns (TapeIn memory) {

    Stream storage stream = streams[tag(c0,t0,c1,t1)];

    mapping (uint => TapeIn) storage tape 
      = t0 < t1 ? stream.tape0 : stream.tape1;

    return tape[ix];

  }

  function datas (
    uint16 c0,
    address t0,
    uint16 c1,
    address t1
  ) public view returns (TapeData memory) {

    Stream storage stream = streams[tag(c0,t0,c1,t1)];

    return t0 < t1 ? stream.data0 : stream.data1;

  }    

  receive() external payable {}

}

