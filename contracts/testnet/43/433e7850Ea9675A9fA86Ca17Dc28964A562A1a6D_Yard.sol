// import "hardhat/console.sol";


import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/interfaces/IERC721.sol";
import "./Fish.sol";



contract Yard is ERC20 {

     using SafeMath for uint256;

    uint256 public MAX_YARD_SUPPLY = 32000000000000000000000000000 ether;
    string private TOKEN_NAME = "YARD";
    string private TOKEN_SYMBOL = "YARD";

    address public CAPTAIN_CONTRACT;
    address public FISH_CONTRACT;
    address public VEFISH_CONTRACT;

    uint256 public BOOSTER_MULTIPLIER = 1;

    uint256 public FISH_FARMING_FACTOR = 3;
    uint256 public FISH_SWAP_FACTOR = 12; 

    uint256 public VEFISH_FARMING_FACTOR = 40;
  



    event Minted(address owner, uint256 numberOfFeed);
    event StakedFish(address owner, uint256 numberOfFish);
    event FishSwap(address owner, uint256 numberOfYard);
    event UnstakedFish(address owner, uint256 numberOfFish);

    struct FishStake {
        address user;
        uint32 since;
        uint256 amount;
    }

    struct VeFishStake {
        address user;
        uint32 since;
        uint256 amount;
    }

    mapping(address => FishStake) public fishStakeHolders;
    uint256 public totalFishStaked;
    address[] public _allFishStakeHolders;
    mapping(address => uint256) private _allFishStakeHoldersIndex;

    mapping(address => VeFishStake) public VeFishStakeHolders;
    uint256 public totalVeFishStaked;
    address[] public _allVeFishStakeHolders;
    mapping(address => uint256) private _allVeFishStakeHoldersIndex;

    event FishStaked(address user, uint256 amount);
    event FishUnStaked(address user, uint256 amount);

    event VeFishStaked(address user, uint256 amount);
    event VeFishUnStaked(address user, uint256 amount);


    constructor(address _captainContract, address _fishContract )
        ERC20(TOKEN_NAME, TOKEN_SYMBOL)
    {
        CAPTAIN_CONTRACT = _captainContract;
        FISH_CONTRACT = _fishContract;

    }

    function _upsertFishStaking(
        address user,
        uint256 amount
    ) internal {
        require(user != address(0), "EMPTY ADDRESS");
        FishStake memory fish = fishStakeHolders[user];

        if(fish.user == address(0)) {
            _allFishStakeHoldersIndex[user] = _allFishStakeHolders.length;
            _allFishStakeHolders.push(user);
        }

        uint256 previousFish = fish.amount;

        fish.user = user;
        fish.amount = amount;
        fish.since = uint32(block.timestamp);

        fishStakeHolders[user] = fish;
        totalFishStaked = totalFishStaked - previousFish + amount;
        emit FishStaked(user,amount);
    }

        function _upsertVeFishStaking(
        address user,
        uint256 amount
    ) internal {
        require(user != address(0), "EMPTY ADDRESS");
        VeFishStake memory VeFishArr = VeFishStakeHolders[user];

        if(VeFishArr.user == address(0)) {
            _allVeFishStakeHoldersIndex[user] = _allVeFishStakeHolders.length;
            _allVeFishStakeHolders.push(user);
        }

        uint256 previousVeFish = VeFishArr.amount;

        VeFishArr.user = user;
        VeFishArr.amount = amount;
        VeFishArr.since = uint32(block.timestamp);

        VeFishStakeHolders[user] = VeFishArr;
        totalVeFishStaked = totalVeFishStaked - previousVeFish + amount;
        emit VeFishStaked(user,amount);
    }

    function _unstakingFish(address user) internal {

        VeFishStake memory VeFishArr = VeFishStakeHolders[user];
        require(VeFishArr.user != address(0), "EMPTY ADDRESS");
        totalVeFishStaked = totalVeFishStaked - VeFishArr.amount;
        _removeUserFromVeFishEnumeration(user);
        delete VeFishStakeHolders[user];
        emit VeFishUnStaked(user, VeFishArr.amount);

    }

    function withdrawAllFishAndClaimYard() external {
        FishStake memory fish = fishStakeHolders[msg.sender];
        require(fish.user != address(0), "MOT STAKED FISH");

        uint256 projection = claimableView(msg.sender);

        if(projection >0) {
            _mintYard(msg.sender, projection);
        } 

        if(fish.amount > 0 ) {
            uint256 afterBurned = (fish.amount * 11) / 12;
            Fish fishContract = Fish(FISH_CONTRACT);
            fishContract.mintFish(msg.sender, afterBurned);
            emit UnstakedFish(msg.sender, afterBurned);
        }

        _unstakingFish(msg.sender);


    }

   

    function _removeUserFromFishEnumeration(address user) private {
        uint256 lastUserIndex = _allFishStakeHolders.length -1;
        uint256 currentUserIndex = _allFishStakeHoldersIndex[user];

        address lastUser = _allFishStakeHolders[lastUserIndex];

        _allFishStakeHolders[currentUserIndex] = lastUser;
        _allFishStakeHoldersIndex[lastUser] = currentUserIndex;

        delete _allFishStakeHoldersIndex[user];
        _allFishStakeHolders.pop();
    }

    function _removeUserFromVeFishEnumeration(address user) private {
        uint256 lastUserIndex = _allVeFishStakeHolders.length -1;
        uint256 currentUserIndex = _allVeFishStakeHoldersIndex[user];

        address lastUser = _allVeFishStakeHolders[lastUserIndex];

        _allVeFishStakeHolders[currentUserIndex] = lastUser;
        _allVeFishStakeHoldersIndex[lastUser] = currentUserIndex;

        delete _allVeFishStakeHoldersIndex[user];
        _allVeFishStakeHolders.pop();
    }

    function fishStake(uint256 amount) external {
        require(amount > 0, "NEED FISH");
        Fish fishContract = Fish(FISH_CONTRACT);
        uint256 available = fishContract.balanceOf(msg.sender);
        require(available >= amount, "NOT ENOUGH FISH");
        FishStake memory existingFish = fishStakeHolders[msg.sender];
        if(existingFish.amount > 0) {
            uint256 projection = claimableView(msg.sender);
            _mint(msg.sender, projection);
            emit Minted(msg.sender, amount);
            _upsertFishStaking(msg.sender, existingFish.amount + amount);
        } else {
            _upsertFishStaking(msg.sender, amount);
        }
        fishContract.burnFish(msg.sender, amount);
        emit StakedFish(msg.sender, amount);
    }

    function veFishStake(uint256 amount) external {
        require(amount > 0, "NEED FISH");
        Fish fishContract = Fish(FISH_CONTRACT);
        uint256 available = fishContract.balanceOf(msg.sender);
        require(available >= amount, "NOT ENOUGH FISH");
        VeFishStake memory VeFishArr = VeFishStakeHolders[msg.sender];
        if(VeFishArr.amount > 0) {
            uint256 projection = claimableView(msg.sender);
            _mint(msg.sender, projection);
            emit Minted(msg.sender, amount);
            _upsertVeFishStaking(msg.sender, VeFishArr.amount + amount);
        } else {
            _upsertVeFishStaking(msg.sender, amount);
        }
        fishContract.burnFish(msg.sender, amount);
        emit StakedFish(msg.sender, amount);
    }

    function claimableView(address user) public view returns (uint256) {
 

        uint256 totalClaim = 0;
        uint256 deltaSeconds = 0;

        FishStake memory fish = fishStakeHolders[user];

        if (fish.user != address(0)) {

            if(fish.amount > 0) {

            deltaSeconds = block.timestamp - fish.since;
            totalClaim = ( (fish.amount * FISH_FARMING_FACTOR ) * ((deltaSeconds * 10000000000) / 86400) * BOOSTER_MULTIPLIER ) / 100000000;
    

            }

        }

        VeFishStake memory veFish = VeFishStakeHolders[user];

        if (veFish.user != address(0)) {

              if(veFish.amount > 0) {

               deltaSeconds = block.timestamp - veFish.since;
               totalClaim = totalClaim + ((veFish.amount * VEFISH_FARMING_FACTOR * 1e18) *((deltaSeconds * 10000000000) / 86400) * BOOSTER_MULTIPLIER)/ 100000000;

              }
        }

        return totalClaim;

    }

  

  



    function feedCaptain(uint256 tokenId, uint256 yardAmount) external {
        require(yardAmount >0 , "YOU NEED MORE YARD");
        IERC721 captainContract = IERC721(CAPTAIN_CONTRACT);
        require(captainContract.ownerOf(tokenId) == msg.sender, "YOU ARE NOT OWNER CAPTAIN");
        require(balanceOf(msg.sender) >= yardAmount, "NOT ENOUGH YARD");

        Fish fishContract = Fish(FISH_CONTRACT);
        (uint24 ton, , , , ) = fishContract.stakedCaptainList(tokenId);
        require(ton > 0, "NOT STAKED CAPTAIN");

        _burn(msg.sender, yardAmount);
        fishContract.feedCaptain(tokenId, yardAmount);


        
    }

    function unStakeFish(uint256 amount) external {
        require(amount > 0, "MUST BE MORE THAN 0");
        FishStake memory fish = fishStakeHolders[msg.sender];
        require(fish.user != address(0), "NOT STAKED FISH");
        require(amount <= fish.amount, "OVERDRAWN FISH");
        Fish fishContract = Fish(FISH_CONTRACT);
        _upsertFishStaking(msg.sender, fish.amount - amount);
        uint256 afterBurned = (amount * 11) / 12;
        fishContract.mintFish(msg.sender, afterBurned);
        emit UnstakedFish(msg.sender, afterBurned); 
    }

    function unStakeVeFish(uint256 amount) external {
        require(amount > 0, "MUST BE MORE THAN 0");
        VeFishStake memory VeFishArr = VeFishStakeHolders[msg.sender];
        require(VeFishArr.user != address(0), "NOT STAKED FISH");
        require(amount <= VeFishArr.amount, "OVERDRAWN VEFISH");
        Fish fishContract = Fish(FISH_CONTRACT);
        _upsertVeFishStaking(msg.sender, VeFishArr.amount - amount);
        uint256 afterBurned = (amount * 3) / 10;
        fishContract.mintFish(msg.sender, afterBurned);
        emit UnstakedFish(msg.sender, afterBurned); 
    }

    function swapFishForYard(uint256 fishAmt) external {
        require(fishAmt > 0, "MUST BE MORE THAN 0 FISH");

        Fish fishContract = Fish(FISH_CONTRACT);
        fishContract.burnFish(msg.sender, fishAmt);

     
        _mintYard(msg.sender, fishAmt * FISH_SWAP_FACTOR);
        emit FishSwap(msg.sender, fishAmt * FISH_SWAP_FACTOR * 1e18);

    }
    
    function _mintYard(address sender, uint256 yardAmount) internal {
        require(totalSupply() + yardAmount*1e18 < MAX_YARD_SUPPLY, "OVER MAX SUPPLY");
        _mint(sender, yardAmount*1e18);
        emit Minted(sender, yardAmount*1e18);
    }

    function mintYard(address sender, uint256 yardAmount) external  {
        _mintYard(sender, yardAmount);
    }

    function claimYard()  external {


        uint256 projection = claimableView(msg.sender);

        if (projection > 0) {

             FishStake memory fish = fishStakeHolders[msg.sender];
             _upsertFishStaking(msg.sender, fish.amount);
             VeFishStake memory VeFish = VeFishStakeHolders[msg.sender];
             _upsertVeFishStaking(msg.sender, VeFish.amount);
            _mintYard(msg.sender, projection) ;

        }

    }


    function updateBoosterMultiplier(uint256 _value) external  {
        BOOSTER_MULTIPLIER = _value;
    }

    /**
     * Admin : updates how much feed you get per staked egg (e.g. 3x).
     */
    function updateFishStakeFactor(uint256 _value) external  {
        FISH_FARMING_FACTOR = _value;
    }

     function updateVeFishStakeFactor(uint256 _value) external  {
        FISH_FARMING_FACTOR = _value;
    }

    /**
     * Admin : updates the multiplier for swapping (burning) egg for feed (e.g. 12x).
     */
    function updateYardSwapFactor(uint256 _value) external  {
        FISH_SWAP_FACTOR = _value;
    }

    /**
     * Admin : updates the maximum available feed supply.
     */
    function updateMaxYardSupply(uint256 _value) external  {
        MAX_YARD_SUPPLY = _value;
    } 

    function totalFishHolder() public view returns (uint256) {
        return _allFishStakeHolders.length;
    }

    function getFishHolderByIndex(uint256 index) internal view returns (address) {
        return _allFishStakeHolders[index];
    }

    function rebalanceStakingPool(uint256 from, uint256 to) external  {
        for (uint256 i = from; i <= to; i++) {
            address holderAddress = getFishHolderByIndex(i);

            uint256 pendingClaim = claimableView(holderAddress);
            FishStake memory fish = fishStakeHolders[holderAddress];

            _mint(holderAddress, pendingClaim);
            emit Minted(holderAddress, pendingClaim);

            _upsertFishStaking(holderAddress, fish.amount);
        }
    }




}

