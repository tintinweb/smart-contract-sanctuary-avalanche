 import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./Captain.sol";
import "./Authorizable.sol";

contract Fish is ERC20, Authorizable {
    /// @custom:oz-upgrades-unsafe-allow constructor


    using SafeMath for uint256;

    string private TOKEN_NAME = "FISH";

    string private TOKEN_SYMBOL = "FISH";

    address public CAPTAIN_CONTRACT;

    uint256 public BASE_HOLDER_FISH = 750000000000000000;
    uint256 public FISH_PER_DAY_PER_TON = 250000000000000000;
    uint256 public COOLDOWN_BASE = 100000000000000000000; 
    uint256 public COOLDOWN_BASE_FACTOR = 100000000000000000000; 
    uint256 public COOLDOWN_CD_IN_SECS = 86400; 
    uint256 public LEVELING_BASE = 25;
    uint256 public LEVELING_RATE = 2;
    uint256 public COOLDOWN_RATE = 3600; 

     struct StakedCaptainDetail {
        uint24 ton;
        uint32 sinceTs;
        uint32 lastSkippedTs;
        uint48 eatenAmount;
        uint32 cooldownTs;
    }

    uint40 public totalTon;
    uint16 public totalStakedCaptain;
    mapping(address => uint24) public yourTotalTon;

    StakedCaptainDetail[100001] public stakedCaptainList;

    event Minted(address owner, uint256 fishAmt);
    event Burned(address owner, uint256 fishAmt);
    event Staked(uint256 tid, uint256 ts);
    event UnStaked(uint256 tid, uint256 ts);

    constructor(address _captainContract) ERC20(TOKEN_NAME, TOKEN_SYMBOL) {
        CAPTAIN_CONTRACT = _captainContract;

    }

    function feedLevelingRate(uint256 ton) public view returns (uint256) {
        return LEVELING_BASE * ((ton / 100)**LEVELING_RATE);
    }

    function cooldownRate(uint256 ton) public view returns (uint256) {
        // need to divide the kg by 100

        return (ton / 100) * COOLDOWN_RATE;
    }


  

     function claimableFishView(uint256 tokenId) public view returns (uint256) {
        StakedCaptainDetail memory c = stakedCaptainList[tokenId];
        if (c.ton > 0) {
            uint256 fishPerDay = ((FISH_PER_DAY_PER_TON * (c.ton / 100)) + BASE_HOLDER_FISH);
            uint256 deltaSeconds = block.timestamp - c.sinceTs;
            return deltaSeconds * (fishPerDay / 86400);
        } else {
            return 0;
        }
    }

    function getEstFish(uint24 ton) public view returns(uint256) {
              uint256 fishPerDay = ((FISH_PER_DAY_PER_TON * (ton/ 100)) + BASE_HOLDER_FISH);
              return fishPerDay;
    }

      function getAllMyStakedCaptainIds(address walletAddress) public view returns (uint256[] memory) {
        Captain x = Captain(CAPTAIN_CONTRACT);
        uint256 captainCount = x.balanceOf(walletAddress);
        uint256[] memory tokenIds = new uint256[](captainCount);
        uint256 counter = 0;
        for (uint256 i = 0; i < captainCount; i++) {
            uint256 tokenId = x.tokenOfOwnerByIndex(walletAddress, i);
            StakedCaptainDetail memory captain = stakedCaptainList[tokenId];
            if (captain.ton > 0) {
                tokenIds[counter] = tokenId;
                counter++;
            }
        }
        return tokenIds;
    }

     

    function getBoost(uint256 ton) public view returns(uint256) {
        return ( ton/100) * COOLDOWN_BASE_FACTOR;
    } 

    function getFeedLevel(uint256 ton) public view returns(uint256) {

       return LEVELING_BASE * (( ton/100) ** LEVELING_RATE);
    }

    function levelUpCaptain(uint256 tokenId) external {
        StakedCaptainDetail memory captain = stakedCaptainList[tokenId];
        require(captain.ton > 0, "NOT STAKED CAPTAIN");

         Captain captainContract = Captain(CAPTAIN_CONTRACT);

         require(captain.eatenAmount >= getFeedLevel(captain.ton), "NEED MORE YARD");
         require(block.timestamp >= captain.cooldownTs, "COOLDOWN NOT MET");

         captain.ton = captain.ton + 100;
         captain.eatenAmount = 0;
         captain.cooldownTs = uint32(block.timestamp + cooldownRate(captain.ton));
         stakedCaptainList[tokenId] = captain;

         totalTon += uint24(100);
         yourTotalTon[msg.sender] += uint24(100);
         

         captainContract.setTon(tokenId, captain.ton);
       
    }



    function _burnFish(address sender, uint256 fishAmount) internal {
        require(balanceOf(sender) >= fishAmount, "NOT ENOUGH FISH");
        _burn(sender,fishAmount);
        emit Burned(sender, fishAmount);
    }

    function burnFish(address sender, uint256 fishAmount) external {
        _burnFish(sender,fishAmount*1e18);
    }

    function skipBoost(uint256 tokenId, uint256 fishAmount) external {

        StakedCaptainDetail memory captain = stakedCaptainList[tokenId];
        require(captain.ton != 0, "CAPTAIN NOT STAKED");

        uint32 ts = uint32(block.timestamp);
        //uint256 walletBalance = balanceOf(msg.sender);
        //require(walletBalance >= fishAmount*1e18, "NOT ENOUGH FISH IN WALLET");
        require(fishAmount*1e18 >= 10000000000000000000, "NOT ENOUGH FISH TO SKIP");
        require(captain.lastSkippedTs + COOLDOWN_CD_IN_SECS <= ts, "BLOCKED BY 24HR COOLDOWN");


        _burnFish(msg.sender, fishAmount*1e18);

        captain.cooldownTs = ts;
        captain.lastSkippedTs = ts;

        stakedCaptainList[tokenId] = captain;

    }

    function feedCaptain(uint256 tokenId, uint256 feedAmount) external {

           StakedCaptainDetail memory captain = stakedCaptainList[tokenId];
           require(captain.ton > 0, "NOT STAKED CAPTAIN");
           require(feedAmount >0 ,"NEED TO YARD");

           captain.eatenAmount = uint48(feedAmount) + captain.eatenAmount;
           stakedCaptainList[tokenId] = captain;

         
    }



    function mintFish(address sender, uint256 fishAmount) external  {
        _mint(sender, fishAmount*1e18);
        emit Minted(sender, fishAmount*1e18);

    }

    function _stakeCaptain(uint256 tokenId) internal {

           Captain captainContract = Captain(CAPTAIN_CONTRACT);
           require(captainContract.ownerOf(tokenId) == msg.sender,"YOU ARE NOT CAPTAIN NFT OWNER");

           (, , , , , , , uint256 ton,) = captainContract.allCaptainMetadata(tokenId);

           StakedCaptainDetail memory captain = stakedCaptainList[tokenId];
           uint32 ts = uint32(block.timestamp);
           if(stakedCaptainList[tokenId].ton == 0) {
               stakedCaptainList[tokenId] = StakedCaptainDetail(
                   uint24(ton),
                   ts,
                   captain.lastSkippedTs > 0 ? captain.lastSkippedTs : uint32(ts- COOLDOWN_CD_IN_SECS),
                   uint48(0),
                   uint32(ts) + uint32(cooldownRate(ton))
               );

            totalStakedCaptain += 1;
            totalTon += uint24(ton);
            yourTotalTon[msg.sender] += uint24(captain.ton);
            console.log(yourTotalTon[msg.sender]);
            captainContract.setStake(tokenId,true);

            

            emit Staked(tokenId, block.timestamp);

           }



    }

    function stakeCaptain(uint256[] calldata tokenIds) external {
        for(uint256 i=0; i< tokenIds.length; i++) {
            _stakeCaptain(tokenIds[i]);
        }
    }

    function _unstakeCaptain(uint256 tokenId) internal {

        Captain captainContract = Captain(CAPTAIN_CONTRACT);
        require(captainContract.ownerOf(tokenId) == msg.sender,"YOU ARE NOT CAPTAIN NFT OWNER");

        StakedCaptainDetail memory captain = stakedCaptainList[tokenId];
        if(captain.ton > 0) {
            totalTon -= uint24(captain.ton);
            yourTotalTon[msg.sender] -= uint24(captain.ton);
            totalStakedCaptain -= 1;

            captain.ton = 0;
            stakedCaptainList[tokenId] = captain;

            captainContract.setStake(tokenId,false);

            emit UnStaked(tokenId, block.timestamp);
        }

         

    }

    function unstakeCaptain(uint256[] calldata tokenIds) external {
         for (uint256 i = 0; i < tokenIds.length; i++) {
            _unstakeCaptain(tokenIds[i]);
        }
    }

    function _unstakeCaptainMultiple(uint256[] calldata tokenIds) internal {
        for(uint256 i=0; i < tokenIds.length; i++) {
            _unstakeCaptain(tokenIds[i]);
        }

    }

    function claimAndUnstakeAllFish(uint256[] calldata tokenIds) external {
        _claimFish(tokenIds);
        _unstakeCaptainMultiple(tokenIds);
    } 




