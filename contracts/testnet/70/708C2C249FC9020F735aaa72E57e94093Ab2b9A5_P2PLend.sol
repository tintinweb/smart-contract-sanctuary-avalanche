pragma solidity ^0.8.17;
import {IP2PLend} from "../interfaces/IP2PLend.sol";
import {P2PLendStorage} from "./P2PLendStorage.sol";
import {IConfigProvider} from "../interfaces/IConfigProvider.sol";
import {Errors} from "../libraries/helpers/Errors.sol";
import "../openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "../openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import {DataTypes} from "../libraries/types/DataTypes.sol";
import {ERC721HolderUpgradeable} from "../openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
import {IERC721Upgradeable} from "../openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import {IERC20Upgradeable} from "../openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "../openzeppelin/contracts/utils/cryptography/ECDSA.sol";

import {SafeMath} from "../libraries/math/SafeMath.sol";
import {BorrowLogic} from "../libraries/logic/BorrowLogic.sol";
import {GenericLogic} from "../libraries/logic/GenericLogic.sol";
import {IP2PLoan} from "../interfaces/IP2PLoan.sol";

contract P2PLend is
    IP2PLend,
    P2PLendStorage,
    ContextUpgradeable,
    ERC721HolderUpgradeable
{
    using ECDSA for bytes32;
    using SafeMath for uint256;
    IConfigProvider public provider;

    // ======== Constructor =========
    constructor() {}

    receive() external payable {}

    function initialize(IConfigProvider _provider) external initializer {
        __Context_init();
        __ERC721Holder_init();
        // provider
        provider = _provider;
    }

    modifier nonReentrant() {
        // On the first call to nonReentrant, _notEntered will be true
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");

        // Any calls to nonReentrant after this point will fail
        _status = _ENTERED;

        _;

        // By storing the original value once again, a refund is triggered (see
        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }

    modifier whenNotPaused() {
        _whenNotPaused();
        _;
    }

    modifier onlyFactoryConfigurator() {
        _onlyFactoryConfigurator();
        _;
    }

    function _whenNotPaused() internal view {
        require(!_paused, Errors.LP_IS_PAUSED);
    }

    function _onlyFactoryConfigurator() internal view {
        require(
            IConfigProvider(provider).owner() == _msgSender(),
            Errors.LP_CALLER_NOT_LEND_POOL_CONFIGURATOR
        );
    }

    function acceptOffer(DataTypes.AcceptOfferParams memory params)
        public
        whenNotPaused
        nonReentrant
    {
        require(msg.sender == params.borrower, Errors.LP_BORROWER_INVALID);
        BorrowLogic.executeBorrow(
            address(provider),
            DataTypes.BorrowParams({
                nftAsset: params.nftAsset,
                nftTokenId: params.nftTokenId,
                reserveAsset: params.reserveAsset,
                loanPrincipalAmount: params.loanPrincipalAmount,
                loanDuration: params.loanDuration,
                loanInterestRate: params.loanInterestRate,
                loanAvailableTo: params.loanAvailableTo,
                offerAvailableTo: params.offerAvailableTo,
                borrower: params.borrower,
                lender: params.lender,
                borrowerNonce: params.borrowerNonce,
                lenderNonce: params.lenderNonce,
                borrowerSignature: params.lenderSignature, //don't use
                lenderSignature: params.lenderSignature,
                isAcceptLoan: false,
                isNative: false
            })
        );
        _setNonceIsUsed(params.borrower, params.borrowerNonce);
        _setNonceIsUsed(params.lender, params.lenderNonce);
    }

    function acceptLoan(DataTypes.AcceptLoanParams memory params)
        public
        whenNotPaused
        nonReentrant
    {
        require(msg.sender == params.lender, Errors.LP_LENDER_INVALID);
        BorrowLogic.executeBorrow(
            address(provider),
            DataTypes.BorrowParams({
                nftAsset: params.nftAsset,
                nftTokenId: params.nftTokenId,
                reserveAsset: params.reserveAsset,
                loanPrincipalAmount: params.loanPrincipalAmount,
                loanDuration: params.loanDuration,
                loanInterestRate: params.loanInterestRate,
                loanAvailableTo: params.loanAvailableTo,
                offerAvailableTo: params.offerAvailableTo,
                borrower: params.borrower,
                lender: params.lender,
                borrowerNonce: params.borrowerNonce,
                lenderNonce: params.lenderNonce,
                borrowerSignature: params.borrowerSignature,
                lenderSignature: params.borrowerSignature, //don't use
                isAcceptLoan: true,
                isNative: false
            })
        );
        _setNonceIsUsed(params.borrower, params.borrowerNonce);
        _setNonceIsUsed(params.lender, params.lenderNonce);
    }

    function acceptLoanETH(DataTypes.AcceptLoanParams memory params)
        public
        payable
        whenNotPaused
        nonReentrant
    {
        require(msg.sender == params.lender, Errors.LP_LENDER_INVALID);
        BorrowLogic.executeBorrow(
            address(provider),
            DataTypes.BorrowParams({
                nftAsset: params.nftAsset,
                nftTokenId: params.nftTokenId,
                reserveAsset: params.reserveAsset,
                loanPrincipalAmount: params.loanPrincipalAmount,
                loanDuration: params.loanDuration,
                loanInterestRate: params.loanInterestRate,
                loanAvailableTo: params.loanAvailableTo,
                offerAvailableTo: params.offerAvailableTo,
                borrower: params.borrower,
                lender: params.lender,
                borrowerNonce: params.borrowerNonce,
                lenderNonce: params.lenderNonce,
                borrowerSignature: params.borrowerSignature,
                lenderSignature: params.borrowerSignature, //don't use
                isAcceptLoan: true,
                isNative: true
            })
        );
        _setNonceIsUsed(params.borrower, params.borrowerNonce);
        _setNonceIsUsed(params.lender, params.lenderNonce);
    }

    function repay(uint256 _loanId, uint256 _amount)
        external
        payable
        nonReentrant
    {
        BorrowLogic.executeRepay(
            address(provider),
            DataTypes.ExecuteRepayParams({
                loanId: _loanId,
                amount: _amount,
                initiator: msg.sender
            })
        );
    }

    function liquidate(uint256 _loanId) external nonReentrant {
        BorrowLogic.executeLiquidate(
            address(provider),
            DataTypes.ExecuteLiquidateParams({
                loanId: _loanId,
                initiator: msg.sender
            })
        );
    }

    function checkReserveWhitelist(address reserveAsset)
        external
        view
        override
        returns (bool)
    {
        return reservesWhitelist[reserveAsset];
    }

    function checkNftWhitelist(address nftAsset)
        external
        view
        override
        returns (bool)
    {
        return nftsWhitelist[nftAsset];
    }

    function checkNonceIsUsed(address user, uint256 nonce)
        external
        view
        override
        returns (bool)
    {
        return _nonceHasBeenUsedForUser[user][nonce];
    }

    function _setNonceIsUsed(address user, uint256 nonce) internal {
        _nonceHasBeenUsedForUser[user][nonce] = true;
    }

    function addReserve(address asset)
        external
        override
        onlyFactoryConfigurator
    {
        require(AddressUpgradeable.isContract(asset), Errors.LP_NOT_CONTRACT);
        _addReserveToList(asset);
    }

    function _addReserveToList(address asset) internal {
        require(
            reservesWhitelist[asset] == false,
            Errors.RL_RESERVE_ALREADY_INITIALIZED
        );
        reservesWhitelist[asset] = true;
    }

    function addNftCollection(address nftAddress)
        external
        override
        onlyFactoryConfigurator
    {
        require(
            AddressUpgradeable.isContract(nftAddress),
            Errors.LP_NOT_CONTRACT
        );
        _addNftToList(nftAddress);
        IERC721Upgradeable(nftAddress).setApprovalForAll(
            provider.loanManager(),
            true
        );
        IP2PLoan(provider.loanManager()).initNft(nftAddress);
    }

    function _addNftToList(address nftAddress) internal {
        require(
            nftsWhitelist[nftAddress] == false,
            Errors.LP_NFT_ALREADY_INITIALIZED
        );
        nftsWhitelist[nftAddress] = true;
    }

    function cancelNonce(uint256 nonce) external {
        _setNonceIsUsed(msg.sender, nonce);
        emit CancelNonce(msg.sender, nonce);
    }
}

