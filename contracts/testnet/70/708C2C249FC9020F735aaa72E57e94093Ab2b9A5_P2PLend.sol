pragma solidity ^0.8.17;
import {GenericLogic} from "./GenericLogic.sol";
import {IConfigProvider} from "../../interfaces/IConfigProvider.sol";
import {DataTypes} from "../types/DataTypes.sol";
import {IP2PLend} from "../../interfaces/IP2PLend.sol";
import {IP2PLoan} from "../../interfaces/IP2PLoan.sol";
import {Errors} from "../helpers/Errors.sol";
import {TransferHelper} from "../helpers/TransferHelper.sol";
import {IERC20Upgradeable} from "../../openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import {SafeERC20Upgradeable} from "../../openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import {IERC721Upgradeable} from "../../openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "../../openzeppelin/contracts-upgradeable/token/ERC721/extensions/IERC721EnumerableUpgradeable.sol";
import {SafeMath} from "../math/SafeMath.sol";
import "hardhat/console.sol";

library BorrowLogic {
    using SafeMath for uint256;
    event Borrow(
        address indexed borrower,
        address indexed reserve,
        address indexed nftAsset,
        uint256 nftTokenId,
        uint256 amount,
        uint256 borrowRate,
        uint256 duration,
        address lender,
        uint256 borrowerNonce,
        uint256 lenderNonce,
        uint256 loanId,
        bool isNative
    );

    event Repay(
        address user,
        address indexed borrower,
        address indexed reserve,
        address indexed nftAsset,
        uint256 nftTokenId,
        uint256 amount,
        uint256 interestAmount,
        uint256 feeAmount,
        uint256 loanId
    );

    event Liquidate(
        address user,
        address indexed borrower,
        address indexed reserve,
        address indexed nftAsset,
        uint256 nftTokenId,
        uint256 borrowAmount,
        uint256 loanId
    );

    struct RepayLocalVars {
        address initiator;
        address loanManager;
        address onBehalfOf;
        uint256 loanId;
        bool isUpdate;
        uint256 borrowAmount;
        uint256 repayAmount;
        uint256 interestAmount;
        uint256 feeAmount;
    }

    struct ExecuteBorrowLocalVars {
        uint256 shopId;
        address initiator;
        uint256 ltv;
        uint256 liquidationThreshold;
        uint256 liquidationBonus;
        uint256 loanId;
        address loanAddress;
        uint256 totalSupply;
        uint256 interestRate;
    }

    function executeBorrow(
        address addressesProvider,
        DataTypes.BorrowParams memory params
    ) external {
        _borrow(addressesProvider, params);
    }

    function _borrow(address provider, DataTypes.BorrowParams memory params)
        internal
    {
        // Sanity check loan values.
        require(
            uint256(params.loanDuration) <=
                IConfigProvider(provider).loanMaxDuration(),
            Errors.LP_DURATION_REACH_LIMITED
        );
        require(
            uint256(params.loanDuration) != 0,
            Errors.LP_DURATION_CAN_NOT_ZERO
        );

        require(
            params.loanAvailableTo >= block.timestamp,
            Errors.LP_AVAILABLE_TO_EXPIRED
        );

        require(
            params.offerAvailableTo >= block.timestamp,
            Errors.LP_AVAILABLE_TO_EXPIRED
        );

        require(
            IP2PLend(IConfigProvider(provider).lendManager())
                .checkReserveWhitelist(params.reserveAsset),
            Errors.LP_RESERVE_NOT_IN_WHITELIST
        );
        require(
            IP2PLend(IConfigProvider(provider).lendManager()).checkNftWhitelist(
                params.nftAsset
            ),
            Errors.LP_NFT_NOT_IN_WHITELIST
        );

        //verify lender signature
        require(
            !IP2PLend(IConfigProvider(provider).lendManager()).checkNonceIsUsed(
                params.lender,
                params.lenderNonce
            ),
            Errors.LP_LENDER_NONCE_INVALID
        );
        if (!params.isAcceptLoan) {
            //borrower accept lender's offer => check lender signature
            params.isNative = false;
            require(
                GenericLogic.isValidLenderSignature(
                    params.loanPrincipalAmount,
                    params.loanDuration,
                    params.loanInterestRate,
                    params.offerAvailableTo,
                    params.nftAsset,
                    params.nftTokenId,
                    params.reserveAsset,
                    params.lender,
                    params.lenderNonce,
                    params.borrower,
                    params.borrowerNonce,
                    params.lenderSignature
                ),
                Errors.LP_LENDER_INVALID_SIGNATURE
            );
        }

        //verify borrower signature
        require(
            !IP2PLend(IConfigProvider(provider).lendManager()).checkNonceIsUsed(
                params.borrower,
                params.borrowerNonce
            ),
            Errors.LP_BORROWER_NONCE_INVALID
        );
        if (params.isAcceptLoan) {
            //lender accept loan => check borrower signature
            require(
                GenericLogic.isValidBorrowerSignature(
                    params.loanPrincipalAmount,
                    params.loanDuration,
                    params.loanInterestRate,
                    params.loanAvailableTo,
                    params.nftAsset,
                    params.nftTokenId,
                    params.reserveAsset,
                    params.borrower,
                    params.borrowerNonce,
                    params.borrowerSignature
                ),
                Errors.LP_BORROWER_INVALID_SIGNATURE
            );
        }
        uint256 loanId = IP2PLoan(IConfigProvider(provider).loanManager())
            .getCollateralLoanId(params.nftAsset, params.nftTokenId);

        if (loanId == 0) {
            IERC721Upgradeable(params.nftAsset).safeTransferFrom(
                params.borrower,
                address(this), // shopFactory
                params.nftTokenId
            );

            loanId = IP2PLoan(IConfigProvider(provider).loanManager())
                .createLoan(params);
        } else {
            revert("not supported");
        }

        if (params.reserveAsset == IConfigProvider(provider).weth()) {
            if (params.isAcceptLoan && params.isNative) {
                require(
                    msg.value == params.loanPrincipalAmount,
                    Errors.LP_INVALID_ETH_AMOUNT
                );
                //transfer eth to borrower
                TransferHelper.safeTransferETH(
                    IConfigProvider(provider).weth(),
                    params.borrower,
                    params.loanPrincipalAmount
                );
            } else {
                //transfer weth from lender to contract
                IERC20Upgradeable(params.reserveAsset).transferFrom(
                    params.lender,
                    address(this),
                    params.loanPrincipalAmount
                );
                //convert weth to eth and transfer to borrower
                TransferHelper.transferWETH2ETH(
                    IConfigProvider(provider).weth(),
                    params.borrower,
                    params.loanPrincipalAmount
                );
            }
        } else {
            IERC20Upgradeable(params.reserveAsset).transferFrom(
                params.lender,
                params.borrower,
                params.loanPrincipalAmount
            );
        }
        emit Borrow(
            params.borrower,
            params.reserveAsset,
            params.nftAsset,
            params.nftTokenId,
            params.loanPrincipalAmount,
            params.loanInterestRate,
            params.loanDuration,
            params.lender,
            params.borrowerNonce,
            params.lenderNonce,
            loanId,
            params.isNative
        );
    }

    function executeRepay(
        address provider,
        DataTypes.ExecuteRepayParams memory params
    ) external returns (uint256, uint256) {
        return _repay(provider, params);
    }

    function _repay(
        address provider,
        DataTypes.ExecuteRepayParams memory params
    ) internal returns (uint256 repayAmount, uint256 feeAmount) {
        RepayLocalVars memory vars;

        vars.initiator = params.initiator;
        vars.loanId = params.loanId;

        require(vars.loanId != 0, Errors.LP_NFT_IS_NOT_USED_AS_COLLATERAL);

        DataTypes.LoanData memory loan = IP2PLoan(
            IConfigProvider(provider).loanManager()
        ).getLoan(vars.loanId);

        require(params.amount > 0, Errors.VL_INVALID_AMOUNT);

        require(
            loan.loanPrincipalAmount > 0,
            Errors.VL_NO_DEBT_OF_SELECTED_TYPE
        );

        require(
            loan.state == DataTypes.LoanState.Active,
            Errors.LPL_INVALID_LOAN_STATE
        );

        uint256 loanMaturityDate = (uint256(loan.loanStartTime)).add(
            uint256(loan.loanDuration).add(
                IConfigProvider(provider).loanExtraRepayDuration()
            )
        );
        require(block.timestamp <= loanMaturityDate, Errors.LP_LOAN_NOT_ACTIVE);
        vars.borrowAmount = loan.loanPrincipalAmount;
        address lender = loan.lender;

        (vars.repayAmount, vars.interestAmount, vars.feeAmount) = GenericLogic
            .calculateInterestInfo(
                GenericLogic.CalculateInterestInfoVars({
                    principalAmount: loan.loanPrincipalAmount,
                    interestRate: loan.loanInterestRate,
                    platformFeeRate: IConfigProvider(provider).platformFeeRate(),
                    loanStartTime: loan.loanStartTime,
                    loanDuration: loan.loanDuration,
                    interestBlock: IConfigProvider(provider).interestBlock()
                })
            );

        require(params.amount >= vars.repayAmount, Errors.VL_INVALID_AMOUNT);
        IP2PLoan(IConfigProvider(provider).loanManager()).repayLoan(
            vars.initiator,
            vars.loanId,
            vars.repayAmount
        );
        if (loan.reserveAsset == IConfigProvider(provider).weth()) {
            require(
                msg.value == vars.repayAmount,
                Errors.LP_INVALID_ETH_AMOUNT
            );
            if (loan.isNative) {
                // Transfer principal-plus-interest-minus-fees (ETH) to lender
                TransferHelper.safeTransferETH(
                    IConfigProvider(provider).weth(),
                    lender,
                    vars.repayAmount.sub(vars.feeAmount)
                );
            } else {
                // Transfer principal-plus-interest-minus-fees (WETH) to lender
                TransferHelper.transferWETHFromETH(
                    IConfigProvider(provider).weth(),
                    lender,
                    vars.repayAmount.sub(vars.feeAmount)
                );
            }
            // Transfer fees (ETH) to admins
            TransferHelper.safeTransferETH(
                IConfigProvider(provider).weth(),
                IConfigProvider(provider).platformFeeReceiver(),
                vars.feeAmount
            );
        } else {
            // Transfer principal-plus-interest-minus-fees from borrower to lender
            IERC20Upgradeable(loan.reserveAsset).transferFrom(
                loan.borrower,
                lender,
                vars.repayAmount.sub(vars.feeAmount)
            );
            // Transfer fees from borrower to admins
            IERC20Upgradeable(loan.reserveAsset).transferFrom(
                loan.borrower,
                IConfigProvider(provider).platformFeeReceiver(),
                vars.feeAmount
            );
        }

        // Transfer collateral from this contract to borrower.
        IERC721Upgradeable(loan.nftAsset).safeTransferFrom(
            address(this),
            loan.borrower,
            loan.nftTokenId
        );

        emit Repay(
            vars.initiator,
            loan.borrower,
            loan.reserveAsset,
            loan.nftAsset,
            loan.nftTokenId,
            vars.repayAmount,
            vars.interestAmount,
            vars.feeAmount,
            vars.loanId
        );
        repayAmount = vars.repayAmount;
        feeAmount = vars.feeAmount;
    }

    /**
     * @notice Implements the liquidate feature. Through `liquidate()`, users liquidate assets in the protocol.
     * @dev Emits the `Liquidate()` event.
     * @param params The additional parameters needed to execute the liquidate function
     */
    function executeLiquidate(
        address provider,
        DataTypes.ExecuteLiquidateParams memory params
    ) external {
        require(params.loanId != 0, Errors.LP_NFT_IS_NOT_USED_AS_COLLATERAL);

        DataTypes.LoanData memory loan = IP2PLoan(
            IConfigProvider(provider).loanManager()
        ).getLoan(params.loanId);

        require(
            loan.state == DataTypes.LoanState.Active,
            Errors.LP_LOAN_NOT_ACTIVE
        );
        // Ensure that the loan is indeed overdue, since we can only liquidate
        // overdue loans.
        uint256 loanMaturityDate = (uint256(loan.loanStartTime)).add(
            uint256(loan.loanDuration).add(
                IConfigProvider(provider).loanExtraRepayDuration()
            )
        );
        require(
            block.timestamp > loanMaturityDate,
            Errors.LP_LOAN_NOT_OVERDUE_YET
        );

        IP2PLoan(IConfigProvider(provider).loanManager()).liquidateLoan(
            loan.lender,
            loan.loanId,
            loan.loanPrincipalAmount
        );

        // transfer erc721 to lender
        IERC721Upgradeable(loan.nftAsset).safeTransferFrom(
            address(this),
            loan.lender,
            loan.nftTokenId
        );

        emit Liquidate(
            msg.sender,
            loan.borrower,
            loan.reserveAsset,
            loan.nftAsset,
            loan.nftTokenId,
            loan.loanPrincipalAmount,
            loan.loanId
        );
    }
}

