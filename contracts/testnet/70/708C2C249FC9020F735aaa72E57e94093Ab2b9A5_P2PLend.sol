library DataTypes {
    enum LoanState {
        // We need a default that is not 'Created' - this is the zero value
        None,
        // The loan has been initialized, funds have been delivered to the borrower and the collateral is held.
        Active,
        // The loan has been repaid, and the collateral has been returned to the borrower. This is a terminal state.
        Repaid,
        // The loan was delinquent and collateral claimed by the liquidator. This is a terminal state.
        Defaulted
    }
    struct LoanData {
        // A unique identifier for this particular loan, sourced from the
        // continuously increasing parameter totalNumLoans.
        uint256 loanId;
        //the current state of the loan
        LoanState state;
        // The original sum of money transferred from lender to borrower at the
        // beginning of the loan, measured in loanERC20Denomination's smallest units.
        uint256 loanPrincipalAmount;
        // The amount of time (measured in seconds) that can elapse before the
        // lender can liquidate the loan and seize the underlying collateral.
        uint256 loanDuration;
        // The interest rate
        uint256 loanInterestRate;
        // The ERC721 contract of the NFT collateral
        address nftAsset;
        //the id of nft token
        uint256 nftTokenId;
        // The block.timestamp when the loan first began (measured in seconds).
        uint256 loanStartTime;
        // The ERC20 contract of the currency being used as principal/interest
        // for this loan.
        address reserveAsset;
        // The address of the borrower.
        address borrower;
        // The address of the borrower.
        address lender;
        bool isNative;
    }

    struct ExecuteRepayParams {
        address initiator;
        uint256 loanId;
        uint256 amount;
    }

    struct ExecuteLiquidateParams {
        address initiator;
        uint256 loanId;
    }

    struct BorrowParams {
        address nftAsset;
        uint256 nftTokenId;
        address reserveAsset;
        uint256 loanPrincipalAmount;
        uint256 loanDuration;
        uint256 loanInterestRate;
        uint256 loanAvailableTo;
        uint256 offerAvailableTo;
        address borrower;
        address lender;
        uint256 borrowerNonce;
        uint256 lenderNonce;
        bytes borrowerSignature;
        bytes lenderSignature;
        bool isAcceptLoan;
        bool isNative;
    }

    struct AcceptLoanParams {
        address nftAsset;
        uint256 nftTokenId;
        address reserveAsset;
        uint256 loanPrincipalAmount;
        uint256 loanDuration;
        uint256 loanInterestRate;
        uint256 loanAvailableTo;
        uint256 offerAvailableTo;
        address borrower;
        address lender;
        uint256 borrowerNonce;
        uint256 lenderNonce;
        bytes borrowerSignature;
    }

    struct AcceptOfferParams {
        address nftAsset;
        uint256 nftTokenId;
        address reserveAsset;
        uint256 loanPrincipalAmount;
        uint256 loanDuration;
        uint256 loanInterestRate;
        uint256 loanAvailableTo;
        uint256 offerAvailableTo;
        address borrower;
        address lender;
        uint256 borrowerNonce;
        uint256 lenderNonce;
        bytes lenderSignature;
    }
}

