import {IConfigProvider} from "../../interfaces/IConfigProvider.sol";
import {IBNFTRegistry} from "../../interfaces/IBNFTRegistry.sol";
import "../../openzeppelin/contracts/utils/cryptography/ECDSA.sol";

import "hardhat/console.sol";

library GenericLogic {
    using Strings for uint256;
    using SafeMath for uint256;
    using PercentageMath for uint256;
    using ECDSA for bytes32;

    struct CalculateInterestInfoVars {
        uint256 principalAmount;
        uint256 interestRate;
        uint256 platformFeeRate;
        uint256 loanStartTime;
        uint256 loanDuration;
        uint256 interestBlock;
    }

    function calculateInterestInfo(CalculateInterestInfoVars memory vars)
        internal
        view
        returns (
            uint256 totalDebt,
            uint256 interest,
            uint256 platformFee
        )
    {
        uint256 durationSoFarInSeconds = 0;
        if (block.timestamp > vars.loanStartTime) {
            durationSoFarInSeconds = block.timestamp - vars.loanStartTime;
        }
        if (vars.interestBlock == 0) {
            vars.interestBlock = 86400; //1day
        }
        uint256 totalBlock = (vars.loanDuration.div(vars.interestBlock)).add(1);
        uint256 sofarBlock = 0;
        uint256 remainBlock = 0;

        if (durationSoFarInSeconds > vars.loanDuration) {
            sofarBlock = totalBlock;
        } else {
            sofarBlock = (durationSoFarInSeconds.div(vars.interestBlock)).add(
                1
            );
            remainBlock = totalBlock - sofarBlock;
        }

        interest = _computeInterestDue(
            vars.principalAmount,
            vars.interestRate,
            sofarBlock,
            remainBlock,
            vars.interestBlock
        );
        platformFee = vars.principalAmount.mul(vars.platformFeeRate).div(10000);
        totalDebt = vars.principalAmount.add(interest).add(platformFee);
        return (totalDebt, interest, platformFee);
    }

    function _computeInterestDue(
        uint256 principalAmount,
        uint256 rate,
        uint256 sofarBlock,
        uint256 remainBlock,
        uint256 interestBlock
    ) internal pure returns (uint256 interest) {
        //start--------------------repay---------------------end
        //        full interest              1/2 interest
        interest = principalAmount
            .mul(rate)
            .mul(sofarBlock)
            .div(uint256(10000))
            .div(uint256(365 * 86400) / interestBlock);
        interest =
            interest +
            principalAmount
                .mul(rate)
                .mul(remainBlock)
                .div(uint256(10000))
                .div(uint256(365 * 86400) / interestBlock)
                .div(2);
    }

    function getBNftAddress(IConfigProvider provider, address nftAsset)
        internal
        view
        returns (address bNftAddress)
    {
        IBNFTRegistry bnftRegistry = IBNFTRegistry(provider.bnftRegistry());
        bNftAddress = bnftRegistry.getBNFTAddresses(nftAsset);
        return bNftAddress;
    }

    function getChainID() internal view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    function isValidLenderSignature(
        uint256 _loanPrincipalAmount,
        uint256 _loanDuration,
        uint256 _loanInterestRate,
        uint256 _offerAvailableTo,
        address _nftAsset,
        uint256 _nftTokenId,
        address _reserveAsset,
        address _lender,
        uint256 _lenderNonce,
        address _borrower,
        uint256 _borrowerNonce,
        bytes memory _lenderSignature
    ) internal view returns (bool) {
        if (_lender == address(0)) {
            return false;
        } else {
            uint256 chainId;
            chainId = getChainID();
            bytes32 message = keccak256(
                abi.encodePacked(
                    _loanPrincipalAmount,
                    _loanDuration,
                    _loanInterestRate,
                    _offerAvailableTo,
                    _nftAsset,
                    _nftTokenId,
                    _reserveAsset,
                    _lender,
                    _lenderNonce,
                    _borrower,
                    _borrowerNonce,
                    chainId
                )
            );
            bytes32 messageWithEthSignPrefix = message.toEthSignedMessageHash();
            return (messageWithEthSignPrefix.recover(_lenderSignature) ==
                _lender);
        }
    }

    function isValidBorrowerSignature(
        uint256 _loanPrincipalAmount,
        uint256 _loanDuration,
        uint256 _loanInterestRate,
        uint256 _loanAvailableTo,
        address _nftAsset,
        uint256 _nftTokenId,
        address _reserveAsset,
        address _borrower,
        uint256 _borrowerNonce,
        bytes memory _borrowerSignature
    ) internal view returns (bool) {
        if (_borrower == address(0)) {
            return false;
        } else {
            uint256 chainId;
            chainId = getChainID();
            bytes32 message = keccak256(
                abi.encodePacked(
                    _loanPrincipalAmount,
                    _loanDuration,
                    _loanInterestRate,
                    _loanAvailableTo,
                    _nftAsset,
                    _nftTokenId,
                    _reserveAsset,
                    _borrower,
                    _borrowerNonce,
                    chainId
                )
            );
            bytes32 messageWithEthSignPrefix = message.toEthSignedMessageHash();
            return (messageWithEthSignPrefix.recover(_borrowerSignature) ==
                _borrower);
        }
    }
}

